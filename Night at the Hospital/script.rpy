    # lu "We'll see."
    lu "Ya veremos."

# game/script.rpy:996
translate spanish cuddle_ending_3d2d4698:

    # lu "Let's try it on some of the doors we couldn't get into before."
    lu "Deberíamos probar las puertas que no se abrieron antes."

# game/script.rpy:997
translate spanish cuddle_ending_5df6edec:

    # fr "All right."
    fr "De acuerdo."

