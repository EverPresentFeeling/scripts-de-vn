<xscript>
  <debug editor="&quot;C:\Program Files (x86)\sakura\sakura.exe&quot; &quot;%src%&quot; -Y=%line%" src="D:\zombies\Series Build\開発データ\開発環境\vol別\vol0\script\text\牧浦・妄想h.txt"/>
  <code>
    <description @l="1" text="牧浦・妄想Ｈ"/>
    <wait_chgmv/>
    <bg @l="6" effect="cross_fade" src="bg03a" time="1000"/>
    <bgm @l="7" src="bgm09" volume="1."/>
    <msg @l="9" char_id="-1" frame_id="-1" name="" text="Makiura se despertó de un sueño superficial justo al&#xA;amanecer." voices=""/>
    <msg @l="10" char_id="-1" frame_id="-1" name="" text="Se sintió muy aletargada." voices=""/>
    <msg @l="11" char_id="-1" frame_id="-1" name="" text=" Las noches inquietas no eran nada&#xA;nuevo para ella, pero la noche anterior había sido&#xA;especialmente mala." voices="" add="true"/>
    <msg @l="12" char_id="-1" frame_id="-1" name="" page_feed="true" text="La salida más reciente de Yuusuke pesaba en su mente." voices=""/>
    <page @l="14" fade="300"/>
    <char @l="15" id="mku_" pos="auto" src="mku_aace"/>
    <msg @l="17" char_id="-1" frame_id="-1" name="" text="(Realmente estoy preocupada por él)." voices="maku01000"/>
    <msg @l="19" char_id="-1" frame_id="-1" name="" text="Yuusuke había ido a buscar un barco." voices=""/>
    <msg @l="20" char_id="-1" frame_id="-1" name="" text="Ella sabía que estaría bien." voices=""/>
    <msg @l="21" char_id="-1" frame_id="-1" name="" text=" Era peligroso, pero era capaz." voices="" add="true"/>
    <msg @l="22" char_id="-1" frame_id="-1" name="" text="Y, sin embargo, no podía deshacerse de sus preocupaciones." voices=""/>
    <msg @l="23" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;Había pasado toda la noche anterior preocupada por lo que&#xA;pasaría si se metía en problemas." voices=""/>
    <clear @l="25" class="char"/>
    <msg @l="28" char_id="-1" frame_id="-1" name="" text="“...”." voices="maku01001"/>
    <msg @l="30" char_id="-1" frame_id="-1" name="" page_feed="true" text="Suspiró." voices=""/>
    <page @l="32" fade="300"/>
    <msg @l="34" char_id="-1" frame_id="-1" name="" text="(No debería actuar así, ahora mismo no. Hay otras cosas más&#xA;importantes)." voices="maku01002"/>
    <msg @l="36" char_id="-1" frame_id="-1" name="" text="Sí, estaba preocupada por Yuusuke." voices=""/>
    <msg @l="37" add="true" char_id="-1" frame_id="-1" name="" text=" Pero había otras&#xA;innumerables cosas que necesitaban su atención." voices=""/>
    <msg @l="38" add="true" char_id="-1" frame_id="-1" name="" text=" Preocuparse&#xA;no la llevaría a ninguna parte." voices=""/>
    <msg @l="39" char_id="-1" frame_id="-1" name="" page_feed="true" text="Tras repasar el programa del día, Makiura se puso a trabajar." voices=""/>
    <page @l="41" fade="300"/>
    <wait_chgmv/>
    <bg @l="42" effect="cross_fade" src="black" time="1000"/>
    <wait_chgmv/>
    <bg @l="43" effect="cross_fade" src="bg03b" time="1000"/>
    <msg @l="46" char_id="-1" frame_id="-1" name="" text="“Qué día tan tranquilo”." voices="maku01003"/>
    <msg @l="48" char_id="-1" frame_id="-1" name="" text="Se enfrascó tanto en su trabajo que, antes de darse cuenta,&#xA;ya había pasado el mediodía." voices=""/>
    <msg @l="49" char_id="-1" frame_id="-1" name="" text=" Las cosas habían estado&#xA;ocupadas, pero no había surgido nada inesperado." voices="" add="true"/>
    <msg @l="50" char_id="-1" frame_id="-1" name="" text="Últimamente, todos los días eran un torbellino de caos en el&#xA;que nunca sabía lo que iba a ocurrir a continuación." voices=""/>
    <msg @l="51" char_id="-1" frame_id="-1" name="" text="Sin embargo, ese día parecía estar inusualmente tranquilo.&#xA;Una tregua en la tormenta." voices=""/>
    <msg @l="52" char_id="-1" frame_id="-1" name="" page_feed="true" text="El tiempo antes de la pandemia habría pasado de forma muy&#xA;similar, pero..." voices=""/>
    <page @l="54" fade="300"/>
    <msg @l="56" char_id="-1" frame_id="-1" name="" text="“Takemura-san...”." voices="maku01004"/>
    <msg @l="58" char_id="-1" frame_id="-1" name="" text="Se encontró murmurando." voices=""/>
    <msg @l="59" char_id="-1" frame_id="-1" name="" text="Quizás era la única que tenía un momento de respiro." voices=""/>
    <msg @l="60" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;¿Estaba Yuusuke a salvo? " voices=""/>
    <msg @l="61" char_id="-1" frame_id="-1" name="" page_feed="true" text="El momento de tranquilidad le permitió dedicar tiempo a sus&#xA;pensamientos, que revivieron las ansias de la mañana." voices=""/>
    <page @l="63" fade="300"/>
    <char @l="65" id="srt_" pos="auto" src="srt_aaaa"/>
    <stopbgm @l="66" fadeout="1000"/>
    <msg @l="68" char_id="-1" frame_id="-1" name="" text="“¿Doctora?”." voices="shir00300"/>
    <begin_trans @l="70" time="200"/>
    <clear @l="71" class="char"/>
    <char @l="72" id="mku_" pos="left" src="mku_aacf"/>
    <char @l="73" id="srt_" pos="right" src="srt_aaaa"/>
    <end_trans @l="74"/>
    <msg @l="76" char_id="-1" frame_id="-1" name="" text="“¡¿Oh, er, sí?!”." voices="maku01005"/>
    <bgm @l="78" src="bgm04" volume="1."/>
    <msg @l="80" char_id="-1" frame_id="-1" name="" text="Makiura se giró ante la repentina interrupción." voices=""/>
    <msg @l="81" add="true" char_id="-1" frame_id="-1" name="" text=" Allí estaba&#xA;Shiratani, de la división de comunicaciones." voices=""/>
    <begin_trans @l="83" effect="cross_fade" time="200"/>
    <char @l="84" id="mku_" pos="left" src="mku_aacc"/>
    <char @l="85" id="srt_" pos="right" src="srt_aaae"/>
    <end_trans @l="86"/>
    <msg @l="88" char_id="-1" frame_id="-1" name="" text="“¿Se siente cansada? ¿Quiere que le traiga un café?”." voices="shir00301"/>
    <msg @l="89" char_id="-1" frame_id="-1" name="" text="“Oh, sí, por favor, si no te importa”." voices="maku01006"/>
    <msg @l="91" char_id="-1" frame_id="-1" name="" text="Shiratani había entrado en la habitación sin que Makiura se&#xA;diera cuenta en absoluto." voices=""/>
    <msg @l="92" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Se reprendió a sí misma por haberse&#xA;despistado." voices=""/>
    <page @l="94" fade="300"/>
    <clear @l="95" layer="srt_"/>
    <msg @l="97" char_id="-1" frame_id="-1" name="" text="Shiratani dejó una bolsa de papel en un rincón de la&#xA;habitación antes de dirigirse a preparar el café." voices=""/>
    <char @l="99" id="mku_" pos="left" src="mku_aaca"/>
    <msg @l="101" char_id="-1" frame_id="-1" name="" text="(¿Qué puede ser?)." voices="maku01007"/>
    <msg @l="103" char_id="-1" frame_id="-1" name="" text="Makiura ladeó la cabeza, pero no pudo vislumbrar lo que había&#xA;dentro de la bolsa." voices=""/>
    <msg @l="104" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Se preguntó qué sería, pero le pareció&#xA;grosero asomarse al interior sin permiso." voices="" add="true"/>
    <clear @l="106" class="char"/>
    <page @l="107" fade="300"/>
    <begin_trans @l="108" effect="cross_fade" time="200"/>
    <char @l="109" id="mku_" pos="left" src="mku_aacd"/>
    <char @l="110" id="srt_" pos="right" src="srt_aaae"/>
    <end_trans @l="111"/>
    <msg @l="113" char_id="-1" frame_id="-1" name="" text="“Aquí tiene su café, doctora”." voices="shir00302"/>
    <msg @l="115" char_id="-1" frame_id="-1" name="" text="Shiratani le entregó una taza." voices=""/>
    <msg @l="116" add="true" char_id="-1" frame_id="-1" name="" text=" Luego recogió su bolsa de&#xA;papel y se sentó en la silla junto a Makiura." voices=""/>
    <msg @l="119" char_id="-1" frame_id="-1" name="" text="“Es bueno que hoy no esté tan ocupada”." voices="shir00303"/>
    <msg @l="120" char_id="-1" frame_id="-1" name="" text="“Sí”." voices="maku01008"/>
    <msg @l="121" char_id="-1" frame_id="-1" name="" text="“Hemos recibido el contacto de Takemura. Todavía está&#xA;buscando un barco, pero aparentemente no ha habido ningún&#xA;problema hasta ahora”." voices="shir00304"/>
    <char @l="123" id="mku_" pos="left" src="mku_abcq"/>
    <msg @l="125" char_id="-1" frame_id="-1" name="" text="“Uf. Es bueno escuchar eso”." voices="maku01009"/>
    <msg @l="127" char_id="-1" frame_id="-1" name="" text="Respiró aliviada y una pequeña nube de vapor salió de su&#xA;café." voices=""/>
    <char @l="129" id="srt_" pos="right" src="srt_aaaf"/>
    <msg @l="131" char_id="-1" frame_id="-1" name="" text="“Je, je”." voices="shir00305"/>
    <msg @l="133" char_id="-1" frame_id="-1" name="" page_feed="true" text="Shiratani soltó una risita." voices=""/>
    <page @l="135" fade="300"/>
    <begin_trans @l="136" time="200"/>
    <char @l="137" id="mku_" pos="left" src="mku_aace"/>
    <char @l="138" id="srt_" pos="right" src="srt_aaae"/>
    <end_trans @l="139"/>
    <msg @l="141" char_id="-1" frame_id="-1" name="" text="“Oh, lo siento”." voices="maku01010"/>
    <msg @l="142" char_id="-1" frame_id="-1" name="" text="“No hay nada que disculpar. Es exactamente la reacción que&#xA;esperaba”." voices="shir00306"/>
    <char @l="144" id="mku_" pos="left" src="mku_aaco"/>
    <msg @l="146" char_id="-1" frame_id="-1" name="" text="“Preferiría que no te burlaras de mí”." voices="maku01011"/>
    <msg @l="148" char_id="-1" frame_id="-1" name="" text="Makiura empezó a sonrojarse." voices=""/>
    <msg @l="149" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Shiratani se inclinó para&#xA;susurrarle al oído." voices=""/>
    <page @l="151" fade="300"/>
    <begin_trans @l="152" time="200"/>
    <clear @l="153" class="char"/>
    <char @l="154" id="l_sr" pos="auto" src="l_srt_aaae"/>
    <end_trans @l="155"/>
    <msg @l="157" char_id="-1" frame_id="-1" name="" text="“Estoy segura de que su novio vendría corriendo si viera esa&#xA;mirada en su cara”." voices="shir00307"/>
    <clear @l="159" class="char"/>
    <char @l="160" id="l_mk" pos="auto" src="l_mku_aack"/>
    <shake @l="161" amp="20" class="all" cycle="1" duration="200" type="random"/>
    <msg @l="163" char_id="-1" frame_id="-1" name="" text="“¡¿Mi qué?!”." voices="maku01012"/>
    <msg @l="165" char_id="-1" frame_id="-1" name="" page_feed="true" text="Makiura casi escupe su café." voices=""/>
    <page @l="167" fade="300"/>
    <clear @l="168" class="char"/>
    <begin_trans @l="169" time="200"/>
    <char @l="170" id="mku_" pos="left" src="mku_aack"/>
    <char @l="171" id="srt_" pos="right" src="srt_aaaf"/>
    <end_trans @l="172"/>
    <msg @l="174" char_id="-1" frame_id="-1" name="" text="“Je, je, je. Será mejor que lo amarre mientras pueda,&#xA;doctora. Parece del tipo que tiene ojos errantes”." voices="shir00308"/>
    <char @l="176" id="mku_" pos="auto" src="mku_aace"/>
    <msg @l="178" char_id="-1" frame_id="-1" name="" text="“¿Qué...?”." voices="maku01013"/>
    <msg @l="180" char_id="-1" frame_id="-1" name="" text="Makiura trató de ocultar su malestar." voices=""/>
    <msg @l="181" char_id="-1" frame_id="-1" name="" text="Puede que Shiratani no haya dicho su nombre, pero Makiura&#xA;estaba segura de que con 'novio' solo podía referirse a&#xA;Yuusuke." voices=""/>
    <msg @l="182" add="true" char_id="-1" frame_id="-1" name="" text=" Y Shiratani estaba siendo muy franca con todo el&#xA;asunto." voices=""/>
    <char @l="184" id="srt_" pos="auto" src="srt_aaae"/>
    <msg @l="186" char_id="-1" frame_id="-1" name="" text="“He elegido el traje perfecto para usted, doctora. Sé que le&#xA;va a encantar”." voices="shir00309"/>
    <clear @l="188" layer="srt_"/>
    <msg @l="190" char_id="-1" frame_id="-1" name="" page_feed="true" text="Shiratani empujó su bolsa de papel a los brazos de la Dra.&#xA;Makiura, que ya se había puesto roja, y se bebió el resto&#xA;del café de un trago." voices=""/>
    <page @l="192" fade="300"/>
    <clear @l="193" class="char"/>
    <char @l="194" id="srt_" pos="auto" src="srt_aaaf"/>
    <msg @l="196" char_id="-1" frame_id="-1" name="" text="“¡Que tenga muy buena suerte!”." voices="shir00310"/>
    <clear @l="198" class="char"/>
    <msg @l="200" char_id="-1" frame_id="-1" name="" text="Con una sonrisa de oreja a oreja, Shiratani salió de la&#xA;habitación." voices=""/>
    <se @l="202" buf="1" src="se004" volume="1."/>
    <char @l="203" id="mku_" pos="auto" src="mku_aack"/>
    <msg @l="205" char_id="-1" frame_id="-1" name="" text="“Oye, espera, yo...”." voices="maku01014"/>
    <msg @l="207" char_id="-1" frame_id="-1" name="" page_feed="true" text="Makiura se quedó con un café a medio terminar y una&#xA;misteriosa bolsa de papel." voices=""/>
    <page @l="209" fade="300"/>
    <stopbgm @l="210" fadeout="2000"/>
    <clear @l="211" class="char"/>
    <char @l="212" id="l_mk" pos="auto" src="l_mku_aace"/>
    <msg @l="214" char_id="-1" frame_id="-1" name="" text="“¿Qué diablos puede ser esto?”." voices="maku01015"/>
    <msg @l="216" char_id="-1" frame_id="-1" name="" text="Dejó la taza y miró dentro de la bolsa." voices=""/>
    <char @l="218" id="l_mk" pos="auto" src="l_mku_aacf"/>
    <msg @l="220" char_id="-1" frame_id="-1" name="" text="“¿Eh?”." voices="maku01016"/>
    <msg @l="222" char_id="-1" frame_id="-1" name="" text="Makiura expresó su incertidumbre." voices=""/>
    <bgm @l="224" src="bgm05" volume="1."/>
    <se @l="225" buf="1" src="se062" volume="1."/>
    <msg @l="227" char_id="-1" frame_id="-1" name="" text="Dentro de la bolsa había nada menos que un uniforme de&#xA;enfermera, demasiado sexy." voices=""/>
    <msg @l="228" char_id="-1" frame_id="-1" name="" text="Era un solo vestido, en lugar de uno con falda separada. Y&#xA;todo el conjunto estaba allí; hasta la gorra." voices=""/>
    <stopse @l="230" buf="1" fadeout="1000"/>
    <char @l="231" id="l_mk" pos="auto" src="l_mku_abco"/>
    <msg @l="233" char_id="-1" frame_id="-1" name="" text="“Shiratani-san, ¿por qué me has dado esto?”." voices="maku01017"/>
    <msg @l="235" char_id="-1" frame_id="-1" name="" text="Se había referido a eso como el “traje perfecto”." voices=""/>
    <msg @l="236" char_id="-1" frame_id="-1" name="" page_feed="true" text=" En otras&#xA;palabras, le estaba diciendo a Makiura que se lo pusiera y&#xA;fuera a ver a Yuusuke." voices="" add="true"/>
    <page @l="238" fade="300"/>
    <clear @l="239" class="char"/>
    <msg @l="241" char_id="-1" frame_id="-1" name="" text="Makiura recordó una conversación que había tenido con algunos&#xA;colegas en el pasado." voices=""/>
    <msg @l="242" add="true" char_id="-1" frame_id="-1" name="" text=" Algo sobre cómo todos los hombres&#xA;fantasean con mujeres en ciertos uniformes." voices=""/>
    <msg @l="243" char_id="-1" frame_id="-1" name="" text=" Y cómo, entre la&#xA;galaxia de uniformes sexys, los uniformes de enfermera eran&#xA;lo mejor de lo mejor." voices="" add="true"/>
    <msg @l="245" char_id="-1" frame_id="-1" name="" text="(¿Takemura también estará interesado en esto?)." voices="maku01018"/>
    <msg @l="247" char_id="-1" frame_id="-1" name="" text="Tal vez Shiratani había conseguido alguna información que&#xA;decía que a Yuusuke le gustaban los uniformes." voices=""/>
    <msg @l="248" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Eso explicaría&#xA;por qué le había dado este traje a Makiura." voices=""/>
    <page @l="250" fade="300"/>
    <char @l="251" id="l_mk" pos="auto" src="l_mku_abco"/>
    <msg @l="253" char_id="-1" frame_id="-1" name="" text="(Pero no hay manera de que me ponga esto. ¡Es demasiado&#xA;embarazoso!)." voices="maku01019"/>
    <msg @l="255" char_id="-1" frame_id="-1" name="" text="Aunque se sentía decepcionada por haber defraudado a&#xA;Shiratani, simplemente no tenía el valor de enfrentarse a&#xA;Yuusuke con ese traje." voices=""/>
    <clear @l="257" class="char"/>
    <se @l="258" buf="1" src="se062" volume="1."/>
    <msg @l="260" add="true" char_id="-1" frame_id="-1" name="" text=" Así que lo dobló de nuevo y lo&#xA;devolvió a la bolsa." voices=""/>
    <msg @l="261" char_id="-1" frame_id="-1" name="" text="Suspirando, miró el reloj." voices=""/>
    <stopse @l="263" buf="1" fadeout="1000"/>
    <char @l="264" id="l_mk" pos="auto" src="l_mku_aace"/>
    <msg @l="266" char_id="-1" frame_id="-1" name="" text="“Probablemente todavía tengo tiempo hasta que Takemura-san&#xA;regrese”." voices="maku01020"/>
    <msg @l="268" char_id="-1" frame_id="-1" name="" text="Una leve somnolencia empezaba a apoderarse de ella,&#xA;probablemente debido a la falta de sueño de la noche&#xA;anterior." voices=""/>
    <msg @l="269" char_id="-1" frame_id="-1" name="" text="Gracias a Yuusuke, últimamente le resultaba más fácil dormir.&#xA;Pero, una insomne como Makiura, sabía que lo mejor era&#xA;aprovechar cualquier oportunidad que le diera su cuerpo." voices=""/>
    <msg @l="271" char_id="-1" frame_id="-1" name="" text="“Solo... echaré una pequeña siesta”." voices="maku01021"/>
    <clear @l="273" class="char"/>
    <msg @l="275" char_id="-1" frame_id="-1" name="" text="Makiura se inclinó hacia adelante sobre su escritorio..." voices=""/>
    <msg @l="276" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" y se&#xA;durmió en un instante." voices=""/>
    <page @l="278" fade="300"/>
    <stopbgm @l="279" fadeout="2000"/>
    <flash @l="280" count="1" fade="500" image="black"/>
    <flash @l="281" count="1" fade="1000" image="black"/>
    <wait_chgmv/>
    <bg @l="283" src="black" time="2000"/>
    <wait @l="284" time="1000"/>
    <msg @l="286" char_id="-1" frame_id="-1" name="" text="“Sayaka. ¡Sayaka!”." voices="shir00311"/>
    <msg @l="288" char_id="-1" frame_id="-1" name="" text="Una voz familiar con una frase desconocida." voices=""/>
    <msg @l="289" char_id="-1" frame_id="-1" name="" text=" La voz pertenecía&#xA;a Shiratani, pero algo estaba mal." voices="" add="true"/>
    <msg @l="290" char_id="-1" frame_id="-1" name="" text="Makiura ahogó un pequeño bostezo mientras se incorporaba de&#xA;su escritorio." voices=""/>
    <msg @l="292" char_id="-1" frame_id="-1" name="" page_feed="true" text="(Debo haberme quedado dormida)." voices="maku01022"/>
    <page @l="294" fade="300"/>
    <wait_chgmv/>
    <bg @l="295" effect="cross_fade" src="bg31a" time="2000"/>
    <bgm @l="296" src="bgm05" volume="1."/>
    <msg @l="299" char_id="-1" frame_id="-1" name="" text="Vio a Shiratani de pie junto a ella." voices=""/>
    <msg @l="300" char_id="-1" frame_id="-1" name="" text=" Obviamente, había&#xA;intentado despertar a Makiura, pero por alguna razón&#xA;llevaba un traje de enfermera." voices="" add="true"/>
    <msg @l="301" char_id="-1" frame_id="-1" name="" text=" Con la boca abierta, Makiura&#xA;preguntó..." voices="" add="true"/>
    <msg @l="303" char_id="-1" frame_id="-1" name="" text="“¿Por qué estás vestida así?”." voices="maku01023"/>
    <msg @l="304" char_id="-1" frame_id="-1" name="" text="“¿Qué quieres decir, Sayaka?”." voices="shir00312"/>
    <msg @l="306" char_id="-1" frame_id="-1" name="" text="Sayaka... Ese era el primer nombre de Makiura, pero Shiratani&#xA;nunca lo había usado antes." voices=""/>
    <msg @l="307" char_id="-1" frame_id="-1" name="" text=" Miró a su alrededor, luego a sí&#xA;misma, y se dio cuenta de que también llevaba un traje de&#xA;enfermera." voices="" add="true"/>
    <char @l="309" id="mku_" pos="auto" src="mku_aaee"/>
    <msg @l="311" char_id="-1" frame_id="-1" name="" text="“Errm...”." voices="maku01024"/>
    <msg @l="313" char_id="-1" frame_id="-1" name="" text="Makiura estaba totalmente confundida, pero Shiratani continuó&#xA;con una sonrisa." voices=""/>
    <msg @l="315" char_id="-1" frame_id="-1" name="" page_feed="true" text="“¿Todavía estás medio dormida?”." voices="shir00313"/>
    <page @l="317" fade="300"/>
    <msg @l="319" char_id="-1" frame_id="-1" name="" text="Ciertamente, su mente aún no funcionaba correctamente." voices=""/>
    <char @l="321" id="mku_" pos="auto" src="mku_aaer"/>
    <msg @l="323" add="true" char_id="-1" frame_id="-1" name="" text=" Makiura&#xA;se frotó la sien en un intento de ahuyentar el cansancio." voices=""/>
    <msg @l="324" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;Todo era tan absurdamente incoherente con lo que ella&#xA;sabía. Nada tenía sentido." voices=""/>
    <msg @l="325" char_id="-1" frame_id="-1" name="" page_feed="true" text="Sin embargo, lenta pero seguramente, sus recuerdos volvieron." voices=""/>
    <page @l="327" fade="300"/>
    <clear @l="328" class="char"/>
    <msg @l="330" char_id="-1" frame_id="-1" name="" text="Era enfermera en el departamento de ginecología." voices=""/>
