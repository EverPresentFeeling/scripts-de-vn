    <char @l="1522" id="mku_" pos="left" src="mku_aack"/>
    <msg @l="1524" char_id="-1" frame_id="-1" name="" text="(Espera, ¿eso fue un sueño?)." voices="maku01134"/>
    <msg @l="1526" char_id="-1" frame_id="-1" name="" text="Finalmente, se dio cuenta de que nada de lo que había visto&#xA;era real." voices=""/>
    <msg @l="1527" char_id="-1" frame_id="-1" name="" page_feed="true" text="Toda la tensión desapareció de ella." voices=""/>
    <page @l="1529" fade="300"/>
    <begin_trans @l="1530" effect="cross_fade" time="500"/>
    <clear @l="1531" class="char"/>
    <char @l="1532" id="l_mk" pos="left" src="l_mku_aacr"/>
    <char @l="1533" id="l_sr" pos="right" src="l_srt_aaab"/>
    <end_trans @l="1534"/>
    <msg @l="1536" char_id="-1" frame_id="-1" name="" text="Shiratani ladeó la cabeza." voices=""/>
    <msg @l="1538" char_id="-1" frame_id="-1" name="" text="“¿Se encuentra bien?”." voices="shir00329"/>
    <msg @l="1540" char_id="-1" frame_id="-1" name="" text="Makiura mantuvo los ojos en el suelo y dijo..." voices=""/>
    <msg @l="1542" char_id="-1" frame_id="-1" name="" text="“Oh, sí... Solo estaba teniendo un sueño extraño, creo”." voices="maku01135"/>
    <char @l="1544" id="l_sr" pos="auto" src="l_srt_aaac"/>
    <msg @l="1546" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Seguro que gemía mucho mientras dormía. ¿Qué estaba&#xA;soñando?”." voices="shir00330"/>
    <page @l="1548" fade="300"/>
    <char @l="1549" id="l_mk" pos="auto" src="l_mku_aaco"/>
    <msg @l="1551" char_id="-1" frame_id="-1" name="" text="“Bueno...”." voices="maku01136"/>
    <msg @l="1553" char_id="-1" frame_id="-1" name="" text="Su cara se puso roja como la remolacha." voices=""/>
    <msg @l="1554" char_id="-1" frame_id="-1" name="" text=" En un intento de&#xA;disimularlo, tomó una taza de café; solo para descubrir que&#xA;estaba vacía. Shiratani la miró con extrañeza." voices="" add="true"/>
    <msg @l="1555" char_id="-1" frame_id="-1" name="" text="&#xA;Afortunadamente, parecía que había decidido dejar pasar el&#xA;comportamiento sospechoso de Makiura." voices="" add="true"/>
    <msg @l="1556" add="true" char_id="-1" frame_id="-1" name="" text=" Shiratani cambió de&#xA;tema." voices=""/>
    <char @l="1558" id="l_sr" pos="auto" src="l_srt_aaab"/>
    <msg @l="1560" char_id="-1" frame_id="-1" name="" text="“Oh, claro, ¿recuerda el traje que le regalé? Resulta que le&#xA;di el equivocado”." voices="shir00331"/>
    <msg @l="1562" char_id="-1" frame_id="-1" name="" page_feed="true" text="Makiura levantó la vista con sorpresa." voices=""/>
    <page @l="1564" fade="300"/>
    <char @l="1565" id="l_mk" pos="auto" src="l_mku_aacn"/>
    <msg @l="1567" char_id="-1" frame_id="-1" name="" text="“¿En serio?”." voices="maku01137"/>
    <char @l="1569" id="l_sr" pos="auto" src="l_srt_aaag"/>
    <msg @l="1571" char_id="-1" frame_id="-1" name="" text="“Sí. Bueno, fue un poco excesivo, ¿no cree?”." voices="shir00332"/>
    <char @l="1573" id="l_mk" pos="auto" src="l_mku_aaco"/>
    <msg @l="1575" char_id="-1" frame_id="-1" name="" text="“Aah... sí... Ciertamente lo fue”." voices="maku01138"/>
    <msg @l="1577" char_id="-1" frame_id="-1" name="" text="Aunque la apuntaran con una pistola, Makiura nunca habría&#xA;confesado que tuvo un sueño sexual con Yuusuke con esa ropa." voices=""/>
    <char @l="1579" id="l_sr" pos="auto" src="l_srt_aaae"/>
    <msg @l="1581" char_id="-1" frame_id="-1" name="" text="“Aunque, traigo buenas noticias. ¡Tengo algo mucho mejor aquí&#xA;mismo!”." voices="shir00333"/>
    <msg @l="1583" char_id="-1" frame_id="-1" name="" page_feed="true" text="Shiratani tomó la bolsa que había dejado anteriormente y, en&#xA;su lugar, dejó la nueva." voices=""/>
    <page @l="1585" fade="300"/>
    <char @l="1586" id="l_mk" pos="auto" src="l_mku_aace"/>
    <msg @l="1588" char_id="-1" frame_id="-1" name="" text="“Oh, gracias...”." voices="maku01139"/>
    <msg @l="1590" char_id="-1" frame_id="-1" name="" text="Makiura aceptó de mala gana la bolsa de papel que Shiratani&#xA;le puso en las manos." voices=""/>
    <begin_trans @l="1592" effect="cross_fade" time="500"/>
    <clear @l="1593" class="char"/>
    <char @l="1594" id="mku_" pos="left" src="mku_aace"/>
    <char @l="1595" id="srt_" pos="right" src="srt_aaaf"/>
    <end_trans @l="1596"/>
    <msg @l="1598" char_id="-1" frame_id="-1" name="" text="“¡Pues, buena suerte con eso! La apoyaré”." voices="shir00334"/>
    <msg @l="1600" char_id="-1" frame_id="-1" name="" page_feed="true" text="Después de dedicarle otra sonrisa soleada, Shiratani salió de&#xA;la habitación." voices=""/>
    <se @l="1602" buf="1" src="se022" volume="1."/>
    <clear @l="1603" layer="srt_"/>
    <waitse @l="1604" buf="1"/>
    <se @l="1605" buf="1" src="se003" volume="1."/>
    <waitse @l="1606" buf="1"/>
    <se @l="1607" buf="1" src="se004" volume="1."/>
    <page @l="1609" fade="300"/>
    <char @l="1610" id="mku_" pos="auto" src="mku_aace"/>
    <msg @l="1612" char_id="-1" frame_id="-1" name="" text="Y así, Makiura se quedó sola y sintiéndose perdida mientras&#xA;miraba con desesperación la nueva bolsa." voices=""/>
    <begin_trans @l="1614" effect="cross_fade" time="500"/>
    <clear @l="1615" layer="mku_"/>
    <char @l="1616" id="l_mk" pos="auto" src="l_mku_aaco"/>
    <end_trans @l="1617"/>
    <msg @l="1619" char_id="-1" frame_id="-1" name="" text="“Otro conjunto, ¿eh?”." voices="maku01140"/>
    <msg @l="1621" char_id="-1" frame_id="-1" name="" text="Recordó el sueño una vez más y empezó a sonrojarse." voices=""/>
    <msg @l="1622" add="true" char_id="-1" frame_id="-1" name="" text=" Qué sueño&#xA;tan loco había sido." voices=""/>
    <msg @l="1623" add="true" char_id="-1" frame_id="-1" name="" text=" Pero, ¿quizás alguna parte oculta de&#xA;ella anhelaba secretamente una conducta tan escandalosa? " voices=""/>
    <msg @l="1624" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;¿Yuusuke podría estar interesado en ella también?" voices=""/>
    <char @l="1626" id="l_mk" pos="auto" src="l_mku_aacr"/>
    <msg @l="1628" char_id="-1" frame_id="-1" name="" text="(¡No, no, no! ¡No soy una pervertida!)." voices="maku01141"/>
    <clear @l="1630" class="char"/>
    <msg @l="1632" char_id="-1" frame_id="-1" name="" text="Makiura se llevó las dos manos a sus ardientes mejillas y&#xA;dejó escapar un enorme suspiro." voices=""/>
    <stopse @l="1634" buf="1" fadeout="1000"/>
    <stopbgm @l="1635" fadeout="2000"/>
