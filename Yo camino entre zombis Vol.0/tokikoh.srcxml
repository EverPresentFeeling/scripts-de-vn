    <bgm @l="33" src="bgm01" volume="1."/>
    <msg @l="35" char_id="-1" frame_id="-1" name="" text="Abrió los ojos y contempló un techo que le resultaba&#xA;familiar." voices=""/>
    <msg @l="36" add="true" char_id="-1" frame_id="-1" name="" text=" El sol de la mañana se colaba por el hueco de las&#xA;cortinas." voices=""/>
    <msg @l="37" char_id="-1" frame_id="-1" name="" text="Se frotó los ojos y se estiró en la cama." voices=""/>
    <msg @l="38" char_id="-1" frame_id="-1" name="" text="Después de haberse quedado despierto hasta tarde la noche&#xA;anterior, estaba terriblemente cansado todavía. Pero, pronto&#xA;llegaría la hora de que sonara el despertador." voices=""/>
    <msg @l="39" char_id="-1" frame_id="-1" name="" page_feed="true" text="Se arrastró lentamente hacia arriba." voices=""/>
    <page @l="41" fade="300"/>
    <se @l="42" buf="1" src="se078" volume="1."/>
    <wait_chgmv/>
    <bg @l="43" effect="cross_fade" src="white" time="500"/>
    <wait @l="44" time="1000"/>
    <wait_chgmv/>
    <bg @l="45" effect="cross_fade" src="bg70a" time="1500"/>
    <msg @l="47" char_id="-1" frame_id="-1" name="" text="“Huaaah...”." voices=""/>
    <msg @l="49" char_id="-1" frame_id="-1" name="" text="Tras un gran bostezo, se dispuso a prepararse." voices=""/>
    <se @l="51" buf="1" src="se131" volume="1."/>
    <msg @l="53" char_id="-1" frame_id="-1" name="" text="Mientras estaba frente al lavabo, lavándose la cara y&#xA;cepillándose los dientes, pensó para sí mismo." voices=""/>
    <msg @l="55" char_id="-1" frame_id="-1" name="" page_feed="true" text="(¿Por qué sigo teniendo ese sueño? Tal vez no he estado&#xA;recibiendo suficiente)." voices=""/>
    <se @l="57" buf="1" src="se130" volume="1."/>
    <wait @l="58" time="500"/>
    <wait_chgmv/>
    <bg @l="60" effect="cross_fade" src="black" time="800"/>
