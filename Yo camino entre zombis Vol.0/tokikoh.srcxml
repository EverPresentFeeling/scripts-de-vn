    <scrnfilter @l="65" src="sepia"/>
    <end_trans @l="66"/>
    <msg @l="68" char_id="-1" frame_id="-1" name="" text="Últimamente había soñado mucho el mismo sueño." voices=""/>
    <msg @l="69" char_id="-1" frame_id="-1" name="" text="Cada vez, violaba a una mujer desconocida." voices=""/>
    <msg @l="70" char_id="-1" frame_id="-1" name="" text=" No opuso&#xA;resistencia y no reaccionó." voices="" add="true"/>
    <msg @l="71" char_id="-1" frame_id="-1" name="" text=" Pero en el sueño, eso no le&#xA;molestaba lo más mínimo. Simplemente siguió golpeando sus&#xA;caderas contra las de ella." voices="" add="true"/>
    <msg @l="72" char_id="-1" frame_id="-1" name="" text="Aunque solo era un sueño, se sentía muy bien, así que al&#xA;principio solo lo había visto como una ventaja añadida a sus&#xA;noches." voices=""/>
    <msg @l="73" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Pero con la misma escena que se repite noche tras&#xA;noche, estaba cada vez más desconcertado." voices="" add="true"/>
    <page @l="75" fade="300"/>
    <begin_trans @l="76" time="1000"/>
