    <wait_chgmv/>
    <bg @l="118" alpha="3" src="bg100a" time="800"/>
    <bgm @l="119" src="bgm04" volume="1."/>
    <msg @l="121" char_id="-1" frame_id="-1" name="" text="Ese día en particular se fue a un sitio por un tiempo." voices=""/>
    <msg @l="122" char_id="-1" frame_id="-1" name="" text=" Solo&#xA;tenía que comprobar el estado de los trabajadores de la&#xA;construcción a su cargo, pero las cosas habían ido&#xA;surgiendo, así que no volvió a la oficina hasta mucho más&#xA;tarde de lo previsto." voices="" add="true"/>
    <msg @l="124" char_id="-1" frame_id="-1" name="" text="“Uf, qué día”." voices=""/>
    <se @l="126" buf="1" src="se128" volume="1."/>
    <msg @l="128" char_id="-1" frame_id="-1" name="" text="Después de dejar su bolsa en el suelo, encendió su&#xA;computadora; listo para ocuparse del papeleo." voices=""/>
    <msg @l="129" char_id="-1" frame_id="-1" name="" text="Mientras se cargaba, se dio cuenta de que tenía un mensaje en&#xA;la aplicación de comunicaciones de la empresa de alguien que&#xA;no conocía." voices=""/>
    <msg @l="130" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Ladeó la cabeza al ver el nombre del remitente." voices=""/>
    <page @l="132" fade="300"/>
    <msg @l="134" char_id="-1" frame_id="-1" name="" text="¿Tokiko Kurose?" voices=""/>
    <msg @l="135" add="true" char_id="-1" frame_id="-1" name="" text=" Nunca había oído ese nombre." voices=""/>
    <msg @l="136" char_id="-1" frame_id="-1" name="" text="Desde luego, no trabajaban en el mismo departamento y no&#xA;recordaba haber trabajado con ella antes." voices=""/>
    <msg @l="137" char_id="-1" frame_id="-1" name="" text="Mientras escaneaba el mensaje, empezó a fruncir el ceño." voices=""/>
    <msg @l="138" char_id="-1" frame_id="-1" name="" text="&#xA;Tras un breve saludo, procedió a exponer de forma&#xA;igualmente breve que tenía un asunto que deseaba tratar." voices="" add="true"/>
    <msg @l="139" char_id="-1" frame_id="-1" name="" page_feed="true" text="&#xA;De nuevo, no era lo que esperaba." voices="" add="true"/>
    <page @l="142" fade="300"/>
    <msg @l="144" char_id="-1" frame_id="-1" name="" text="“¿Umm?”." voices=""/>
    <msg @l="146" char_id="-1" frame_id="-1" name="" text="En ese momento, algo en su mente hizo clic." voices=""/>
    <msg @l="150" char_id="-1" frame_id="-1" name="" text="“Oh, claro... Ella”." voices=""/>
    <msg @l="152" char_id="-1" frame_id="-1" name="" text="Pensó en las nuevas contrataciones de ese año." voices=""/>
    <msg @l="153" char_id="-1" frame_id="-1" name="" text="Entre ellas había una mujer tranquila que llevaba gafas." voices=""/>
    <msg @l="154" char_id="-1" frame_id="-1" name="" text="&#xA;Aunque había sido asignada a un departamento diferente,&#xA;Yuusuke había estado brevemente a cargo de su orientación." voices="" add="true"/>
    <msg @l="155" char_id="-1" frame_id="-1" name="" text="&#xA;Supuso que esa era la razón por la que había pedido&#xA;hablar con él. Dado que era una persona recién contratada,&#xA;probablemente tenía problemas con alguien de la empresa." voices="" add="true"/>
    <msg @l="156" char_id="-1" frame_id="-1" name="" text="O tal vez estaba considerando renunciar, o algo parecido." voices=""/>
    <msg @l="157" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text="&#xA;No era raro." voices=""/>
    <page @l="159" fade="300"/>
    <msg @l="161" char_id="-1" frame_id="-1" name="" text="(Qué fastidio)." voices=""/>
    <se @l="163" buf="1" src="se053" volume="1."/>
    <msg @l="165" char_id="-1" frame_id="-1" name="" text="Por mucho que le apeteciera ignorar el mensaje, preparó una&#xA;respuesta." voices=""/>
    <msg @l="166" char_id="-1" frame_id="-1" name="" text="Tras un momento de reflexión, escribió:" voices=""/>
    <msg @l="167" char_id="-1" frame_id="-1" name="" text=" No pude encontrar&#xA;espacio en mi agenda, y..." voices="" add="true"/>
    <msg @l="168" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" te enviaré otro mensaje cuando&#xA;pueda." voices=""/>
    <stopse @l="170" buf="1" fadeout="1000"/>
    <page @l="171" fade="300"/>
    <msg @l="173" char_id="-1" frame_id="-1" name="" text="“Uf”." voices=""/>
    <msg @l="175" char_id="-1" frame_id="-1" name="" text="Cerró la aplicación de mensajería." voices=""/>
    <wait_chgmv/>
    <bg @l="177" alpha="3" src="black" time="800"/>
    <msg @l="179" char_id="-1" frame_id="-1" name="" text="Pasó aproximadamente una hora." voices=""/>
    <msg @l="180" char_id="-1" frame_id="-1" name="" text="Yuusuke había terminado su jornada de trabajo y, tras anotar&#xA;el horario de mañana en la pizarra, tomó la llave de su&#xA;tarjeta y salió de la oficina." voices=""/>
    <msg @l="181" add="true" char_id="-1" frame_id="-1" name="" text=" Mañana haría la inspección&#xA;preliminar de una nueva obra." voices=""/>
    <msg @l="182" char_id="-1" frame_id="-1" name="" page_feed="true" text="En ese momento ya se había olvidado del mensaje que había&#xA;recibido antes." voices=""/>
    <stopbgm @l="184" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="185" effect="cross_fade" src="black" time="1000"/>
    <page @l="187" fade="300"/>
