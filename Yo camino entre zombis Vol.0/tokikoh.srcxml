    <scrnfilter @l="241" src=""/>
    <wait_chgmv/>
    <bg @l="242" src="bg100a"/>
    <char @l="243" id="l_to" pos="auto" src="l_tok_aajd"/>
    <end_trans @l="244"/>
    <msg @l="246" char_id="-1" frame_id="-1" name="" text="Era la mujer con la que había soñado cada noche." voices=""/>
    <msg @l="247" char_id="-1" frame_id="-1" name="" text="Su rostro nunca había sido visible en sus sueños. No debería&#xA;haberla reconocido. Sin embargo, lo hizo." voices=""/>
    <msg @l="248" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" No había duda,&#xA;esa mujer que tenía delante era la mujer de sus sueños." voices=""/>
    <page @l="250" fade="300"/>
    <stopbgm @l="251" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="252" effect="cross_fade" src="black" time="2000"/>
    <wait @l="253" time="1000"/>
    <bgm @l="255" src="bgm05" volume="1."/>
    <begin_trans @l="256" effect="cross_fade" time="800"/>
    <wait_chgmv/>
    <bg @l="257" src="bg100a"/>
    <char @l="258" id="tok_" pos="auto" src="tok_aajg"/>
    <end_trans @l="259"/>
    <msg @l="261" char_id="-1" frame_id="-1" name="" text="Yuusuke se tomó un momento para recomponerse mientras ella&#xA;hablaba." voices=""/>
    <msg @l="262" char_id="-1" frame_id="-1" name="" text=" Resultó ser Tokiko Kurose, la mujer que había&#xA;enviado el mensaje antes." voices="" add="true"/>
    <msg @l="263" char_id="-1" frame_id="-1" name="" text=" Y cuanto más la miraba, más seguro&#xA;estaba de que era la mujer de sus sueños." voices="" add="true"/>
    <char @l="265" id="tok_" pos="auto" src="tok_aajg"/>
    <msg @l="266" char_id="-1" frame_id="-1" name="" text="“...”." voices="toki00001"/>
    <msg @l="268" char_id="-1" frame_id="-1" name="" text="Tokiko miraba torpemente al suelo mientras se disculpaba por&#xA;haber venido a ver a Yuusuke sin ser invitada, su voz era un&#xA;susurro tan débil que a menudo resultaba inaudible." voices=""/>
    <msg @l="269" char_id="-1" frame_id="-1" name="" text="Yuusuke decidió dejar de lado el tema del sueño y preguntar&#xA;de qué quería hablar." voices=""/>
    <msg @l="270" char_id="-1" frame_id="-1" name="" page_feed="true" text=" No podía echarla cuando ya estaba allí,&#xA;y además quería saber más sobre ella." voices="" add="true"/>
    <page @l="272" fade="300"/>
    <msg @l="274" char_id="-1" frame_id="-1" name="" text="Así que procedió a describir sus quejas." voices=""/>
    <msg @l="275" char_id="-1" frame_id="-1" name="" text="Algunos de los veteranos de su departamento la acosaban&#xA;sexualmente." voices=""/>
    <msg @l="276" add="true" char_id="-1" frame_id="-1" name="" text=" Hablar con su jefe no había servido para mejorar&#xA;el asunto." voices=""/>
    <msg @l="277" char_id="-1" frame_id="-1" name="" text=" Ella tropezó con sus palabras, y Yuusuke tuvo que&#xA;esforzarse para captar lo que decía." voices="" add="true"/>
    <msg @l="278" char_id="-1" frame_id="-1" name="" text="Aunque Yuusuke había sido el encargado de preguntarle qué le&#xA;preocupaba, al escucharla explicarse, se encontró lamentando&#xA;totalmente lo molesto que iba a ser tratar con ella." voices=""/>
    <msg @l="280" char_id="-1" frame_id="-1" name="" text="(¿Por qué yo?)." voices=""/>
    <msg @l="282" char_id="-1" frame_id="-1" name="" text="Él la había ayudado a instalarse cuando se incorporó, pero&#xA;eso fue todo. ¿Tal vez no tenía a nadie más a quien&#xA;recurrir? No parecía la persona más sociable." voices=""/>
    <msg @l="283" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Yuusuke la&#xA;miró de arriba a abajo." voices=""/>
    <page @l="285" fade="300"/>
    <wait_chgmv/>
    <bg @l="286" effect="cross_fade" src="white" time="200"/>
    <wait_chgmv/>
    <bg @l="287" effect="cross_fade" src="bg115b" time="200"/>
    <begin_trans @l="289" time="1200"/>
    <wait_chgmv/>
    <bg @l="290" src="bg100a"/>
    <char @l="291" id="tok_" pos="auto" src="tok_aajg"/>
    <end_trans @l="292"/>
    <msg @l="294" char_id="-1" frame_id="-1" name="" text="Entonces, tuvo un repentino recuerdo de su sueño." voices=""/>
    <msg @l="295" add="true" char_id="-1" frame_id="-1" name="" text=" Recordó&#xA;cómo la había inmovilizado y había tenido sexo con ella a&#xA;gusto." voices=""/>
    <msg @l="296" char_id="-1" frame_id="-1" name="" text="Las visiones íntimas le hicieron sentir, por un momento, una&#xA;lujuria totalmente inapropiada por ella." voices=""/>
    <msg @l="297" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Era como si ya no&#xA;tuviera el control de sí mismo." voices=""/>
    <page @l="299" fade="300"/>
    <se @l="301" buf="1" src="se021" volume="1."/>
    <flash @l="302" count="2" fade="200" image="white"/>
    <msg @l="304" char_id="-1" frame_id="-1" name="" text="(¡Ngh!)." voices=""/>
    <msg @l="306" char_id="-1" frame_id="-1" name="" text="Luchó contra los impulsos con pánico." voices=""/>
    <msg @l="307" add="true" char_id="-1" frame_id="-1" name="" text=" Parecía que sus sueños&#xA;nocturnos estaban teniendo una mala influencia en él." voices=""/>
    <msg @l="309" char_id="-1" frame_id="-1" name="" text="(Es solo un maldito sueño. Esta chica no es mi tipo de todos&#xA;modos)." voices=""/>
    <msg @l="311" char_id="-1" frame_id="-1" name="" text="Se había olvidado por completo de Tokiko después de que la&#xA;asignaran a su división." voices=""/>
    <msg @l="312" add="true" char_id="-1" frame_id="-1" name="" text=" Probablemente era su inconsciente el&#xA;que la hacía manifestarse en sus sueños." voices=""/>
    <msg @l="313" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Personalmente,&#xA;habría preferido una compañía más madura para una cita&#xA;nocturna." voices="" add="true"/>
    <page @l="315" fade="300"/>
    <begin_trans @l="316" time="500"/>
    <clear @l="317" class="char"/>
    <char @l="318" id="l_to" pos="auto" src="l_tok_aajg"/>
    <end_trans @l="319"/>
    <msg @l="321" char_id="-1" frame_id="-1" name="" text="(Espera, ¿esta chica siempre ha tenido este aspecto tan&#xA;horrible?)." voices=""/>
    <msg @l="323" char_id="-1" frame_id="-1" name="" text="Volvió a mirar a Tokiko." voices=""/>
    <msg @l="324" char_id="-1" frame_id="-1" name="" text=" Seguía mirando al suelo, con&#xA;el rostro mortalmente pálido." voices="" add="true"/>
    <msg @l="325" char_id="-1" frame_id="-1" name="" text="Aunque había estado callada&#xA;durante su orientación, él podría haber jurado que tenía un&#xA;aspecto más presentable." voices="" add="true"/>
    <msg @l="327" char_id="-1" frame_id="-1" name="" text="(Umm, será mejor que la eche. Escuchar más de esta mierda&#xA;solo me va a causar problemas)." voices=""/>
    <msg @l="329" char_id="-1" frame_id="-1" name="" text="Si él escuchaba toda la extensión de sus problemas y optaba&#xA;por no hacer nada, entonces se enfrentaría a parte de la&#xA;culpa si la mierda le salpicaba." voices=""/>
    <msg @l="330" add="true" char_id="-1" frame_id="-1" name="" text=" Su mejor opción era&#xA;abandonar mientras estaba en ventaja." voices=""/>
    <msg @l="332" char_id="-1" frame_id="-1" name="" text="(De todos modos, ¿a quién le importa?)." voices=""/>
    <msg @l="334" char_id="-1" frame_id="-1" name="" text="Lanzó un largo suspiro." voices=""/>
    <msg @l="335" char_id="-1" frame_id="-1" name="" page_feed="true" text="Yuusuke se convenció de que esa decisión era lo mejor para&#xA;él." voices=""/>
    <stopbgm @l="337" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="338" effect="cross_fade" src="black" time="1000"/>
    <page @l="339" fade="300"/>
    <wait @l="340" time="1000"/>
    <msg @l="342" char_id="-1" frame_id="-1" name="" page_feed="true" text="Un mes después." voices=""/>
    <page @l="344" fade="300"/>
    <wait_chgmv/>
    <bg @l="345" alpha="4" src="bg100a" time="800"/>
    <bgm @l="346" src="bgm04" volume="1."/>
    <se @l="347" buf="1" src="se053" volume="1."/>
    <msg @l="349" char_id="-1" frame_id="-1" name="" text="“Ugh... Este montón de mierda nunca va a desaparecer,&#xA;¿verdad?”." voices=""/>
    <msg @l="351" char_id="-1" frame_id="-1" name="" text="Un teclado que repiquetea resuena en una oficina que, por lo&#xA;demás, está en silencio." voices=""/>
    <msg @l="352" char_id="-1" frame_id="-1" name="" text=" Yuusuke estaba trabajando horas&#xA;extras, como en los últimos días." voices="" add="true"/>
    <msg @l="353" char_id="-1" frame_id="-1" name="" text="La oficina central se enteró de las denuncias de acoso sexual&#xA;y despidió a todo el personal problemático." voices=""/>
    <msg @l="354" add="true" char_id="-1" frame_id="-1" name="" text=" El supervisor que&#xA;había ignorado la denuncia inicial también fue destituido de&#xA;su cargo." voices=""/>
    <msg @l="355" char_id="-1" frame_id="-1" name="" text="Yuusuke había calculado mal." voices=""/>
    <msg @l="356" char_id="-1" frame_id="-1" name="" text="Los enormes cambios de personal resultantes habían fastidiado&#xA;toda la planificación de sus proyectos. Para arreglar la&#xA;situación era necesario que alguien hiciera turnos extra, y&#xA;ese alguien era Yuusuke." voices=""/>
    <msg @l="357" add="true" char_id="-1" frame_id="-1" name="" text=" Levantó la vista del portátil y se&#xA;masajeó la sien." voices=""/>
    <msg @l="358" char_id="-1" frame_id="-1" name="" page_feed="true" text="Su cerebro estaba a punto de explotar por el estrés de todas&#xA;las horas extras." voices=""/>
    <page @l="360" fade="300"/>
    <msg @l="362" char_id="-1" frame_id="-1" name="" text="“Gracias por ayudarme”." voices=""/>
    <char @l="364" id="tok_" pos="auto" src="tok_aajh"/>
    <msg @l="366" char_id="-1" frame_id="-1" name="" text="“No es nada”." voices="toki00002"/>
    <msg @l="368" char_id="-1" frame_id="-1" name="" text="Tokiko, que trabajaba en la mesa de al lado, respondió en voz&#xA;baja sin apartar la vista del monitor." voices=""/>
    <msg @l="369" char_id="-1" frame_id="-1" name="" text="Ningún trabajo pertenecía a su departamento, pero se había&#xA;ofrecido a ayudar, ya que era la causa inmediata del&#xA;desastre." voices=""/>
    <msg @l="370" char_id="-1" frame_id="-1" name="" text=" Él había intentado que se fuera a casa, pero ella&#xA;se había mostrado extrañamente obstinada en quedarse a&#xA;ayudar." voices="" add="true"/>
    <msg @l="371" char_id="-1" frame_id="-1" name="" text="Hacía tiempo que la oscuridad se había instalado en el&#xA;exterior." voices=""/>
    <msg @l="372" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Solo ellos dos se habían quedado después del&#xA;horario de oficina." voices=""/>
    <clear @l="374" layer="tok_"/>
    <page @l="375" fade="300"/>
    <msg @l="377" char_id="-1" frame_id="-1" name="" text="“¡Diablos, que se joda esta mierda!”." voices=""/>
    <wait_chgmv/>
    <bg @l="379" effect="shading_off" src="bg100a" time="1000"/>
    <wait_chgmv/>
    <bg @l="380" effect="shading_off" src="bg100a" time="1000"/>
    <msg @l="382" char_id="-1" frame_id="-1" name="" text="Los ojos de Yuusuke se habían vuelto vidriosos de tanto mirar&#xA;el monitor. Se desplomó en su silla y se llevó las palmas de&#xA;las manos a la frente." voices=""/>
    <msg @l="383" char_id="-1" frame_id="-1" name="" text=" Había pasado el día haciendo más&#xA;inspecciones in situ, dejando todo el papeleo para las horas&#xA;extras." voices="" add="true"/>
    <msg @l="384" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" El sueño había quedado relegado al mínimo que aún&#xA;necesitaba para funcionar." voices=""/>
    <page @l="386" fade="300"/>
    <wait_chgmv/>
    <bg @l="387" alpha="3" src="black" time="800"/>
    <wait_chgmv/>
    <bg @l="388" alpha="3" src="bg100a" time="800"/>
    <se @l="390" buf="1" src="se154" volume="1."/>
    <waitse @l="391" buf="1"/>
    <char @l="393" id="tok_" pos="auto" src="tok_aajd"/>
    <msg @l="395" char_id="-1" frame_id="-1" name="" text="“...”." voices="toki00003"/>
    <msg @l="397" char_id="-1" frame_id="-1" name="" text="Cuando Yuusuke sacó la cena que había comprado para él en una&#xA;tienda, Tokiko sacó su propia comida y empezaron a comer." voices=""/>
    <char @l="399" id="tok_" pos="auto" src="tok_aajg"/>
    <msg @l="401" char_id="-1" frame_id="-1" name="" text="&#xA;Parecía que estaba comiendo las sobras del almuerzo. Cuando&#xA;se dio cuenta de que Yuusuke la miraba, parecía&#xA;increíblemente avergonzada." voices="" add="true"/>
    <clear @l="403" class="char"/>
    <msg @l="405" add="true" char_id="-1" frame_id="-1" name="" text=" Sin embargo, permaneció tan&#xA;silenciosa como siempre." voices=""/>
    <msg @l="406" char_id="-1" frame_id="-1" name="" text="Su voz era tan baja que él tenía que pedirle que repitiera&#xA;todo, y parecía que eso la había hecho decidir que&#xA;renunciaba a hablar por completo." voices=""/>
    <msg @l="407" char_id="-1" frame_id="-1" name="" text=" Sin embargo, tenían trabajo&#xA;que hacer juntos, y el silencio no iba a ayudar a ello.&#xA;Habían optado por comunicarse a través del cliente de&#xA;mensajería de trabajo para hacer las cosas." voices="" add="true"/>
    <msg @l="408" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Las cosas seguían&#xA;siendo profesionales, pero al menos conversaban." voices="" add="true"/>
    <page @l="410" fade="300"/>
    <wait_chgmv/>
    <bg @l="411" alpha="3" src="black" time="800"/>
    <wait_chgmv/>
    <bg @l="412" alpha="3" src="bg100a" time="800"/>
    <se @l="413" buf="1" src="se053" volume="1."/>
    <msg @l="415" char_id="-1" frame_id="-1" name="" text="(...)." voices=""/>
    <msg @l="417" char_id="-1" frame_id="-1" name="" text="Yuusuke trató de combatir el aura sombría de Tokiko mientras&#xA;seguía trabajando." voices=""/>
    <msg @l="418" add="true" char_id="-1" frame_id="-1" name="" text=" Sus sueños donde la violaba habían&#xA;continuado." voices=""/>
    <msg @l="419" add="true" char_id="-1" frame_id="-1" name="" text=" Aunque, ella no era su tipo." voices=""/>
    <msg @l="420" char_id="-1" frame_id="-1" name="" text="Incapaz de entender por qué seguía soñando con ella, empezó a&#xA;preocuparse. Y, esas preocupaciones agravaron su estrés." voices=""/>
    <msg @l="421" char_id="-1" frame_id="-1" name="" text="Le echó una mirada a la chica." voices=""/>
    <msg @l="422" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Estaba sentada, frente a&#xA;su computadora portátil." voices="" add="true"/>
    <page @l="424" fade="300"/>
    <char @l="425" id="l_to" pos="auto" src="l_tok_aaje"/>
    <msg @l="427" char_id="-1" frame_id="-1" name="" text="(Esa postura me mata solo de verla, debo tener una vista de&#xA;mierda)." voices=""/>
    <msg @l="429" char_id="-1" frame_id="-1" name="" text="Sin embargo, por alguna razón, había empezado a encontrarla&#xA;bonita." voices=""/>
    <msg @l="430" add="true" char_id="-1" frame_id="-1" name="" text=" También podía imaginarse fácilmente el aspecto de&#xA;ella bajo ese traje." voices=""/>
    <msg @l="431" add="true" char_id="-1" frame_id="-1" name="" text=" Porque la veía desnuda todas las noches&#xA;cuando la profanaba en sus sueños." voices=""/>
    <msg @l="432" char_id="-1" frame_id="-1" name="" text="Con ese recuerdo de cómo se sentía, un repentino impulso&#xA;lujurioso se apoderó de él." voices=""/>
    <clear @l="434" class="char"/>
    <msg @l="436" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Pero, lo reprimió y siguió&#xA;trabajando." voices=""/>
    <page @l="438" fade="300"/>
    <wait_chgmv/>
    <bg @l="439" alpha="3" src="black" time="800"/>
    <wait_chgmv/>
    <bg @l="440" alpha="3" src="bg100a" time="800"/>
    <se @l="441" buf="1" src="se052" volume="1."/>
    <msg @l="443" char_id="-1" frame_id="-1" name="" text="Por fin, el final estaba a la vista." voices=""/>
    <msg @l="444" add="true" char_id="-1" frame_id="-1" name="" text=" Aunque aún quedaba&#xA;mucho por hacer." voices=""/>
    <msg @l="446" char_id="-1" frame_id="-1" name="" text="“Con un poco de suerte, debería poder hacerse la semana que&#xA;viene”." voices=""/>
    <msg @l="448" char_id="-1" frame_id="-1" name="" text="Tokiko había ayudado a acelerar las cosas, y ya estaban&#xA;casi terminadas." voices=""/>
    <msg @l="450" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Gracias, has sido de gran ayuda”." voices=""/>
    <page @l="452" fade="300"/>
    <char @l="453" id="l_to" pos="auto" src="l_tok_aajh"/>
    <msg @l="455" char_id="-1" frame_id="-1" name="" text="“...”." voices="toki00004"/>
    <msg @l="457" char_id="-1" frame_id="-1" name="" text="Sus labios se perfilaron en una leve sonrisa mientras&#xA;asentía." voices=""/>
    <msg @l="458" char_id="-1" frame_id="-1" name="" text="Yuusuke había notado en la orientación que Tokiko había sido&#xA;rápida en el papeleo." voices=""/>
    <msg @l="459" add="true" char_id="-1" frame_id="-1" name="" text=" No tenía mucho sentido para él por qué&#xA;estaba en ventas." voices=""/>
    <msg @l="461" char_id="-1" frame_id="-1" name="" text="(Está claro que no está hecha para eso)." voices=""/>
    <msg @l="463" char_id="-1" frame_id="-1" name="" text="No es que tuviera ningún poder para cambiar eso, después de&#xA;todo el incidente de acoso." voices=""/>
    <msg @l="464" char_id="-1" frame_id="-1" name="" text=" Probablemente RR.HH. estaba&#xA;teniendo problemas para decidir qué hacer con ella en primer&#xA;lugar. Aunque quizás había una posibilidad de que escucharan&#xA;a Yuusuke, dada su antigüedad." voices="" add="true"/>
    <clear @l="466" layer="l_to"/>
    <msg @l="468" char_id="-1" frame_id="-1" name="" text="(Tal vez lo plantee en algún momento)." voices=""/>
    <msg @l="470" char_id="-1" frame_id="-1" name="" page_feed="true" text="Con eso en mente, se dispuso a terminar el trabajo de la&#xA;noche." voices=""/>
    <wait_chgmv/>
    <bg @l="472" effect="cross_fade" src="black" time="1000"/>
    <page @l="473" fade="300"/>
    <wait @l="474" time="1000"/>
    <wait_chgmv/>
    <bg @l="475" effect="cross_fade" src="bg100a" time="1000"/>
    <msg @l="477" char_id="-1" frame_id="-1" name="" text="Tras volver de un breve descanso, encontró a Tokiko&#xA;desplomada sobre su escritorio. Estaba dormida." voices=""/>
    <msg @l="479" char_id="-1" frame_id="-1" name="" text="“Levántate. Si quieres dormir, hazlo en casa”." voices=""/>
    <msg @l="481" char_id="-1" frame_id="-1" name="" text="Tokiko se había quedado con él durante todas las horas&#xA;extras." voices=""/>
    <msg @l="482" add="true" char_id="-1" frame_id="-1" name="" text=" Probablemente tampoco había dormido lo suficiente." voices=""/>
    <msg @l="483" char_id="-1" frame_id="-1" name="" text="Le sacudió los hombros en un intento de despertarse." voices=""/>
    <msg @l="484" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text="&#xA;Pero justo cuando iba a decir algo más..." voices=""/>
    <page @l="486" fade="300"/>
    <stopbgm @l="487" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="488" effect="cross_fade" src="white" time="200"/>
    <se @l="489" buf="1" src="se021" volume="1."/>
    <wait_chgmv/>
    <bg @l="490" effect="cross_fade" src="bg115b" time="200"/>
    <wait_chgmv/>
    <bg @l="492" effect="cross_fade" src="bg100a" time="1200"/>
    <bgm @l="493" src="bgm06" volume="1."/>
    <msg @l="495" char_id="-1" frame_id="-1" name="" text="La visión de su rostro dormido hizo que su mente regresara al&#xA;sueño." voices=""/>
    <msg @l="497" char_id="-1" frame_id="-1" name="" text="Su aspecto era el mismo que el de entonces." voices=""/>
    <msg @l="498" char_id="-1" frame_id="-1" name="" page_feed="true" text="Esa era la mujer con la que se había acostado innumerables&#xA;veces." voices=""/>
    <page @l="500" fade="300"/>
    <msg @l="502" char_id="-1" frame_id="-1" name="" text="“...”." voices=""/>
    <msg @l="504" char_id="-1" frame_id="-1" name="" text="En ese momento, el diablo pareció tomar el control." voices=""/>
    <msg @l="508" char_id="-1" frame_id="-1" name="" text="La mano de Yuusuke se extendió." voices=""/>
    <msg @l="509" char_id="-1" frame_id="-1" name="" text="Tal vez el cansancio extremo le había nublado el juicio, o&#xA;incluso había difuminado las líneas de la realidad. Tal vez,&#xA;por lo que podía decir, eso era solo una continuación de su&#xA;sueño." voices=""/>
    <se @l="511" buf="1" src="se049" volume="1."/>
    <msg @l="513" add="true" char_id="-1" frame_id="-1" name="" text=" Tokiko no se movió ni siquiera cuando le tocó el&#xA;pecho." voices=""/>
    <msg @l="514" char_id="-1" frame_id="-1" name="" text="Yuusuke empezó a respirar con dificultad por la excitación." voices=""/>
    <msg @l="515" char_id="-1" frame_id="-1" name="" text="&#xA;Su pene estaba dolorosamente erecto, empujando contra&#xA;sus pantalones." voices="" add="true"/>
    <msg @l="516" char_id="-1" frame_id="-1" name="" page_feed="true" text="Tokiko aún no había reaccionado." voices=""/>
    <page @l="518" fade="300"/>
    <msg @l="520" char_id="-1" frame_id="-1" name="" text="Estaba tanteando su ropa, agarrando sus pechos, cuando..." voices=""/>
    <stopbgm @l="522" fadeout="2000"/>
    <shake @l="523" amp="20" class="bg" cycle="1" duration="200" type="random"/>
    <msg @l="525" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;Se estremeció." voices=""/>
    <msg @l="527" char_id="-1" frame_id="-1" name="" text="“Ngh...”." voices="toki00005"/>
    <msg @l="529" char_id="-1" frame_id="-1" name="" text="Antes de que supiera lo que estaba pasando, Tokiko lo miraba&#xA;con miedo." voices=""/>
    <msg @l="530" add="true" char_id="-1" frame_id="-1" name="" text=" Al instante, recuperó el sentido común." voices=""/>
    <msg @l="532" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Lo siento...”." voices=""/>
    <page @l="534" fade="300"/>
    <char @l="535" id="l_to" pos="auto" src="l_tok_aaji"/>
    <clear @l="536" class="char"/>
    <char @l="537" id="tok_" pos="auto" src="tok_aaji"/>
    <msg @l="539" char_id="-1" frame_id="-1" name="" text="Presa del pánico, dio un paso atrás." voices=""/>
    <se @l="541" buf="1" src="se132" volume="1."/>
    <clear @l="542" class="char"/>
    <se @l="543" buf="1" src="se022" volume="1."/>
    <msg @l="545" char_id="-1" frame_id="-1" name="" page_feed="true" text="Tokiko se apresuró a arreglar su ropa, recoger sus cosas y&#xA;emprender la huida." voices=""/>
    <page @l="547" fade="300"/>
    <bgm @l="548" src="bgm09" volume="1."/>
    <msg @l="550" char_id="-1" frame_id="-1" name="" text="(Ahora sí que me he tirado al perro)." voices=""/>
    <msg @l="552" char_id="-1" frame_id="-1" name="" text="Yuusuke agachó la cabeza." voices=""/>
    <msg @l="553" add="true" char_id="-1" frame_id="-1" name="" text=" Había metido la pata hasta el&#xA;fondo." voices=""/>
    <msg @l="554" add="true" char_id="-1" frame_id="-1" name="" text=" Eso había sido más que estúpido;" voices=""/>
    <msg @l="555" add="true" char_id="-1" frame_id="-1" name="" text=" sería el siguiente&#xA;en irse." voices=""/>
    <msg @l="556" char_id="-1" frame_id="-1" name="" text="No podía poner excusas." voices=""/>
    <msg @l="557" char_id="-1" frame_id="-1" name="" text=" Se había sentado un precedente y,&#xA;sin embargo, había ido a hacer la misma idiotez." voices="" add="true"/>
    <msg @l="558" char_id="-1" frame_id="-1" name="" text="Lo despedirían, sin duda." voices=""/>
    <msg @l="559" char_id="-1" frame_id="-1" name="" page_feed="true" text="Yuusuke retomó sus cosas con lentitud y salió a trompicones&#xA;del lugar." voices=""/>
    <page @l="561" fade="300"/>
    <stopse @l="562" buf="1"/>
    <wait_chgmv/>
    <bg @l="563" src="black" time="2000"/>
    <stopbgm @l="564" fadeout="2000"/>
    <wait @l="565" time="1000"/>
    <wait_chgmv/>
    <bg @l="567" alpha="1" src="bg100a" time="800"/>
    <char @l="568" id="tok_" pos="auto" src="tok_aaje"/>
    <bgm @l="569" src="bgm03" volume="1."/>
    <msg @l="571" char_id="-1" frame_id="-1" name="" text="Extrañamente, Tokiko también vino a ayudarle al día&#xA;siguiente." voices=""/>
    <msg @l="573" char_id="-1" frame_id="-1" name="" text="“...”." voices="toki00006"/>
    <clear @l="575" class="char"/>
    <msg @l="577" char_id="-1" frame_id="-1" name="" text="Yuusuke no tenía ni idea de lo que estaba pasando." voices=""/>
    <msg @l="579" char_id="-1" frame_id="-1" name="" text="Pero, por miedo a pinchar el avispero, continuó con su&#xA;trabajo en silencio." voices=""/>
    <se @l="581" buf="1" src="se053" volume="1."/>
    <msg @l="583" char_id="-1" frame_id="-1" name="" text="Tal vez ya había denunciado el incidente a los superiores, y&#xA;era solo cuestión de tiempo que lo despidieran." voices=""/>
    <msg @l="584" char_id="-1" frame_id="-1" name="" text="Sin embargo, no había habido ninguna señal de nada fuera de&#xA;lo normal." voices=""/>
    <msg @l="585" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Nadie lo había llamado, y el día había&#xA;transcurrido exactamente igual que el anterior." voices="" add="true"/>
    <page @l="586" fade="300"/>
    <msg @l="587" char_id="-1" frame_id="-1" name="" text="Tokiko no había dicho una palabra desde que empezó a&#xA;trabajar." voices=""/>
    <msg @l="588" char_id="-1" frame_id="-1" name="" text=" Le envió algunos mensajes y recibió respuestas&#xA;sencillas y prácticas." voices="" add="true"/>
    <msg @l="589" add="true" char_id="-1" frame_id="-1" name="" text=" Siguió trabajando en silencio en la&#xA;mesa de al lado." voices=""/>
    <se @l="591" buf="1" src="se053" volume="1."/>
    <msg @l="593" char_id="-1" frame_id="-1" name="" text="Un tenso silencio se cernía sobre la habitación mientras los&#xA;dos trabajaban." voices=""/>
    <stopse @l="595" buf="1"/>
    <msg @l="597" char_id="-1" frame_id="-1" name="" page_feed="true" text="Al cabo de un rato llegaron a un punto de parada, y Tokiko se&#xA;estiró desde su sitio." voices=""/>
    <page @l="599" fade="300"/>
    <char @l="600" id="l_to" pos="auto" src="l_tok_aajf"/>
    <wait @l="601" time="100"/>
    <clear @l="602" class="char"/>
    <msg @l="604" char_id="-1" frame_id="-1" name="" text="“Mm...”." voices="toki00007"/>
    <msg @l="606" char_id="-1" frame_id="-1" name="" text="Sus enormes pechos sobresalían, haciendo fuerza contra su&#xA;camisa." voices=""/>
    <msg @l="607" char_id="-1" frame_id="-1" name="" text="Yuusuke la observó, paralizado." voices=""/>
    <char @l="609" id="l_to" pos="auto" src="l_tok_aajd"/>
    <msg @l="611" char_id="-1" frame_id="-1" name="" text="“¡Ah!”." voices="toki00008"/>
    <char @l="613" id="l_to" pos="auto" src="l_tok_aajf"/>
    <msg @l="615" char_id="-1" frame_id="-1" name="" text="Tokiko notó su mirada y se apresuró a encorvarse de nuevo&#xA;para ocultar su pecho." voices=""/>
    <msg @l="616" add="true" char_id="-1" frame_id="-1" name="" text=" Ella lo miró con los ojos abatidos." voices=""/>
    <msg @l="617" char_id="-1" frame_id="-1" name="" text="Más que juzgar, parecía simplemente avergonzada." voices=""/>
    <msg @l="618" char_id="-1" frame_id="-1" name="" text="Yuusuke estaba perplejo." voices=""/>
    <msg @l="620" char_id="-1" frame_id="-1" name="" text="(¿Qué pasa con ella? ¿En qué está pensando?)." voices=""/>
    <msg @l="622" char_id="-1" frame_id="-1" name="" text="Tokiko actuaba como si no hubiera pasado nada." voices=""/>
    <msg @l="623" char_id="-1" frame_id="-1" name="" text="Intentó deducir qué podía estar tramando, pero el cansancio y&#xA;la falta de sueño le impedían pensar con claridad." voices=""/>
    <msg @l="624" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Y desde&#xA;luego, no le ayudaba el hecho de que llevara todo el día&#xA;temiendo por su trabajo." voices=""/>
    <page @l="626" fade="300"/>
    <msg @l="627" char_id="-1" frame_id="-1" name="" text="(...)." voices=""/>
    <msg @l="629" char_id="-1" frame_id="-1" name="" text="Pensar resultó ser demasiado molesto. Así que, desesperado,&#xA;Yuusuke se levantó y dijo..." voices=""/>
    <msg @l="631" char_id="-1" frame_id="-1" name="" text="“¿Te importa si echo un vistazo a eso?”." voices=""/>
    <msg @l="633" char_id="-1" frame_id="-1" name="" text="Se acercó por detrás de Tokiko y se inclinó para ver bien su&#xA;portátil." voices=""/>
    <char @l="635" id="l_to" pos="auto" src="l_tok_aaji"/>
    <msg @l="637" char_id="-1" frame_id="-1" name="" page_feed="true" text="“¿Qué?”." voices="toki00009"/>
    <page @l="639" fade="300"/>
    <se @l="641" buf="1" src="se052" volume="1."/>
    <clear @l="642" class="char"/>
    <stopbgm @l="643" fadeout="2000"/>
    <msg @l="645" char_id="-1" frame_id="-1" name="" text="Sin esperar respuesta, le tendió la mano al ratón." voices=""/>
    <msg @l="646" add="true" char_id="-1" frame_id="-1" name="" text=" Estaba&#xA;posicionado justo encima de ella." voices=""/>
    <msg @l="647" char_id="-1" frame_id="-1" name="" text="Desconcertada, Tokiko se quedó rígida como una tabla." voices=""/>
    <msg @l="648" char_id="-1" frame_id="-1" name="" text="Podía sentir su calor donde su pecho tocaba su espalda." voices=""/>
    <msg @l="649" char_id="-1" frame_id="-1" name="" text="&#xA;Mientras él pulsaba la pantalla, Tokiko empezó a relajarse." voices="" add="true"/>
    <msg @l="650" char_id="-1" frame_id="-1" name="" text="No estaba huyendo." voices=""/>
    <msg @l="651" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Y eso hizo que Yuusuke se sintiera&#xA;en conflicto." voices=""/>
    <se @l="653" buf="1" loop="true" src="se021" volume="1."/>
    <page @l="654" fade="300"/>
    <msg @l="656" char_id="-1" frame_id="-1" name="" text="(Espera. Hombre, esto podría ser un problema. Déjalo mientras&#xA;estás tranquilo)." voices=""/>
    <msg @l="658" char_id="-1" frame_id="-1" name="" text="Sería una auténtica estupidez hacer una jugada con una&#xA;compañera de trabajo que ha acudido a él para quejarse de&#xA;un caso de acoso sexual." voices=""/>
    <msg @l="659" add="true" char_id="-1" frame_id="-1" name="" text=" El peligro era evidente." voices=""/>
    <msg @l="660" char_id="-1" frame_id="-1" name="" text="Sin embargo, había pasado tanto tiempo con Tokiko, y ella se&#xA;parecía tanto a la mujer de sus sueños sexuales, que su&#xA;paciencia estaba llegando al límite." voices=""/>
    <msg @l="661" char_id="-1" frame_id="-1" name="" text="Si ella no iba a rechazarlo, entonces..." voices=""/>
    <msg @l="662" char_id="-1" frame_id="-1" name="" page_feed="true" text="Mientras Yuusuke seguía revisando algunos archivos, le puso&#xA;la mano libre en el hombro." voices=""/>
    <page @l="664" fade="300"/>
    <stopse @l="665" buf="1" fadeout="1000"/>
    <msg @l="668" char_id="-1" frame_id="-1" name="" text="“Ah...”." voices="toki00010"/>
    <msg @l="670" char_id="-1" frame_id="-1" name="" text="Se movió ligeramente, pero no reaccionó más que eso." voices=""/>
    <msg @l="671" char_id="-1" frame_id="-1" name="" text="&#xA;Sus labios se apretaron, quizás por la vergüenza, pero no&#xA;dijo nada." voices="" add="true"/>
    <msg @l="672" char_id="-1" frame_id="-1" name="" text="Tomando su falta de objeción como una buena señal, deslizó su&#xA;mano hacia su pecho." voices=""/>
    <msg @l="673" char_id="-1" frame_id="-1" name="" text=" Pudo sentir bien sus voluminosos pechos,&#xA;justo debajo de la suave seda de su blusa." voices="" add="true"/>
    <msg @l="675" char_id="-1" frame_id="-1" name="" text="“Mm...”." voices="toki00011"/>
    <msg @l="677" char_id="-1" frame_id="-1" name="" text="Comenzó a frotar y a recoger los maduros y torneados&#xA;montículos." voices=""/>
    <msg @l="678" char_id="-1" frame_id="-1" name="" text=" Eran muy elásticos, incluso a través de su ropa." voices="" add="true"/>
    <msg @l="679" char_id="-1" frame_id="-1" name="" text="Ya que había llegado tan lejos, no había forma de que&#xA;pudiera hacer pasar sus acciones por algo que no fuera un&#xA;avance sexual, pero Tokiko seguía sin mostrar signos de&#xA;objeción." voices=""/>
    <msg @l="680" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Su cara se enrojeció y agachó la cabeza, pero eso&#xA;fue todo." voices="" add="true"/>
    <label @l="684" id="scene_03"/>
    <page @l="688" fade="300"/>
    <bgm @l="689" src="bgm05" volume="1."/>
    <wait_chgmv/>
    <ev @l="690" src="ev0h04aa" time="800"/>
    <msg @l="692" char_id="-1" frame_id="-1" name="" text="Cuando giró la cabeza, su cabello se deslizó hacia un lado y&#xA;reveló su nuca sonrojada." voices=""/>
    <msg @l="694" char_id="-1" frame_id="-1" name="" text="“Mm, ah, umm...”." voices="toki00012"/>
    <msg @l="696" char_id="-1" frame_id="-1" name="" text="La abundancia de su pecho se acomodó bien en la palma de su&#xA;mano mientras él se frotaba y manoseaba." voices=""/>
    <msg @l="697" add="true" char_id="-1" frame_id="-1" name="" text=" El aroma de su&#xA;champú le hizo cosquillas en la nariz." voices=""/>
    <msg @l="699" char_id="-1" frame_id="-1" name="" text="“¿Qué... pasa con el trabajo?”." voices="toki00013"/>
    <msg @l="701" char_id="-1" frame_id="-1" name="" text="Su voz era apenas un susurro." voices=""/>
    <msg @l="703" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Hemos terminado por hoy. Puedes irte si quieres”." voices=""/>
    <page @l="705" fade="300"/>
    <stopse @l="706" buf="1"/>
    <wait_chgmv/>
    <ev @l="707" src="ev0h04ad" time="0"/>
    <msg @l="709" char_id="-1" frame_id="-1" name="" text="“...”." voices="toki00014"/>
    <msg @l="711" char_id="-1" frame_id="-1" name="" text="Tokiko se puso aún más roja y volvió a mirar al suelo." voices=""/>
    <msg @l="712" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;No intentó escapar como el día anterior." voices=""/>
    <msg @l="713" add="true" char_id="-1" frame_id="-1" name="" text=" Y, eso fue&#xA;suficiente para acabar con las dudas de Yuusuke." voices=""/>
    <msg @l="714" char_id="-1" frame_id="-1" name="" text="Observó su entorno una vez más." voices=""/>
    <msg @l="715" char_id="-1" frame_id="-1" name="" text=" Dado lo tarde que era, no&#xA;había nadie más en la oficina. Pero, el edificio contaba&#xA;con seguridad las 24 horas del día." voices="" add="true"/>
    <msg @l="716" char_id="-1" frame_id="-1" name="" text=" Acababan de pasar en&#xA;su patrulla, así que pasaría un tiempo antes de que volvieran&#xA;a aparecer." voices="" add="true"/>
    <msg @l="717" char_id="-1" frame_id="-1" name="" page_feed="true" text="Aguzó el oído y, al oír el zumbido del portátil, deslizó los&#xA;dedos en la abertura de la blusa." voices=""/>
    <page @l="719" fade="300"/>
    <wait_chgmv/>
    <ev @l="720" src="ev0h04ac" time="0"/>
    <msg @l="722" char_id="-1" frame_id="-1" name="" text="“¡Ah, aah!”." voices="toki00015"/>
    <msg @l="724" char_id="-1" frame_id="-1" name="" text="La piel desnuda parecía aferrarse a él." voices=""/>
    <msg @l="725" char_id="-1" frame_id="-1" name="" text="Su uña arañó un pezón, haciendo que los hombros de Tokiko se&#xA;movieran." voices=""/>
    <msg @l="726" char_id="-1" frame_id="-1" name="" text="El sujetador no podía contener todo el volumen de los pechos,&#xA;que se desbordaban y empujaban contra su tacto." voices=""/>
    <msg @l="727" char_id="-1" frame_id="-1" name="" text=" La prenda, de&#xA;diseño modesto, era una talla más pequeña para Tokiko." voices="" add="true"/>
    <msg @l="728" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text="&#xA;Sus pechos parecían querer liberarse." voices=""/>
    <page @l="730" fade="300"/>
    <wait_chgmv/>
    <ev @l="731" src="ev0h04ab" time="0"/>
    <msg @l="733" char_id="-1" frame_id="-1" name="" text="“¡Um, umm!”." voices="toki00016"/>
    <msg @l="735" char_id="-1" frame_id="-1" name="" text="Al ver cómo intentaba ocultar su vergüenza, Yuusuke&#xA;comprendió." voices=""/>
    <msg @l="736" char_id="-1" frame_id="-1" name="" text=" Llevaba los sujetadores más pequeños a propósito,&#xA;tratando de ocultar lo grande que eran sus pechos." voices="" add="true"/>
    <msg @l="737" char_id="-1" frame_id="-1" name="" text=" Pero ya&#xA;no había nada que los limitara, no ahora que los&#xA;manoseaba directamente." voices="" add="true"/>
    <msg @l="738" char_id="-1" frame_id="-1" name="" text="La sangre acudió a su regione inferior al ver cómo Tokiko se&#xA;retorcía tímidamente ante su contacto, con su regordete&#xA;pecho presionando contra sus dedos." voices=""/>
    <msg @l="739" char_id="-1" frame_id="-1" name="" text=" Levantando un montículo&#xA;entero con la palma de la mano, encontró la carne abultada&#xA;de donde se clavó el sujetador y jugó con ella entre los&#xA;dedos." voices="" add="true"/>
    <msg @l="740" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Los alambres y el relleno del sujetador se sentían&#xA;terriblemente duros en comparación con la suavidad de sus&#xA;pechos." voices=""/>
    <page @l="742" fade="300"/>
    <msg @l="744" char_id="-1" frame_id="-1" name="" text="(Supongo que estos chicos son un obstáculo para ella)." voices=""/>
    <msg @l="746" char_id="-1" frame_id="-1" name="" text="Tal vez se inclinaba mucho porque también estaba preocupada&#xA;por sus enormes melones." voices=""/>
    <msg @l="747" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Se rió internamente mientras&#xA;deslizaba sus dedos dentro del sujetador." voices=""/>
    <page @l="749" fade="300"/>
    <wait_chgmv/>
    <ev @l="750" src="ev0h04af" time="0"/>
    <se @l="751" buf="1" src="se049" volume="1."/>
    <msg @l="753" char_id="-1" frame_id="-1" name="" text="“Oh, no...”." voices="toki00017"/>
    <msg @l="755" char_id="-1" frame_id="-1" name="" text="Tokiko se retorció en sus brazos." voices=""/>
    <msg @l="756" add="true" char_id="-1" frame_id="-1" name="" text=" Los ojos suplicantes de&#xA;la chica encendieron un fuego en su ingle." voices=""/>
    <stopse @l="759" buf="1"/>
    <wait_chgmv/>
    <ev @l="760" src="ev0h04ae" time="0"/>
    <msg @l="762" char_id="-1" frame_id="-1" name="" text="“Aah...”." voices="toki00018"/>
    <msg @l="765" char_id="-1" frame_id="-1" name="" text="Le desabrochó la blusa, dejando escapar todo ese calor&#xA;atrapado en su pecho." voices=""/>
    <msg @l="766" add="true" char_id="-1" frame_id="-1" name="" text=" La fragancia del sudor y del jabón&#xA;salió a su encuentro." voices=""/>
    <msg @l="768" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Su respiración se agitó al ver el&#xA;maduro escote." voices=""/>
    <page @l="770" fade="300"/>
    <se @l="771" buf="1" src="se049" volume="1."/>
    <wait_chgmv/>
    <ev @l="772" effect="cross_fade" src="ev0h04cc" time="800"/>
    <msg @l="774" char_id="-1" frame_id="-1" name="" text="“¡Naah!”." voices="toki00019"/>
    <msg @l="777" char_id="-1" frame_id="-1" name="" text="En sus pechos pálidos, iluminados por la luz artificial de la&#xA;oficina, brillaban las gotas plateadas de sudor." voices=""/>
    <msg @l="778" char_id="-1" frame_id="-1" name="" text=" Se le había&#xA;formado un tinte rojo en el pecho, tal vez por la vergüenza,&#xA;lo que solo lo puso más caliente." voices="" add="true"/>
    <stopse @l="780" buf="1"/>
    <msg @l="782" char_id="-1" frame_id="-1" name="" text="Volvió a agarrarle los pechos por encima del sujetador." voices=""/>
    <msg @l="783" add="true" char_id="-1" frame_id="-1" name="" text="&#xA;Después de varios apretones fuertes, lo desenganchó&#xA;ágilmente de la espalda." voices=""/>
    <msg @l="784" char_id="-1" frame_id="-1" name="" text="Las copas cayeron de los pechos, permitiendo que las&#xA;voluptuosas masas rebotaran libres." voices=""/>
    <msg @l="785" char_id="-1" frame_id="-1" name="" text=" Al haber escapado de sus&#xA;ataduras, parecían haber doblado su tamaño." voices="" add="true"/>
    <msg @l="786" char_id="-1" frame_id="-1" name="" page_feed="true" text="Sus dedos se deslizaron inmediatamente en el hueco." voices=""/>
    <page @l="788" fade="300"/>
    <wait_chgmv/>
    <ev @l="789" src="ev0h04cf" time="0"/>
    <msg @l="791" char_id="-1" frame_id="-1" name="" text="“Aaaaah...”." voices="toki00020"/>
    <msg @l="793" char_id="-1" frame_id="-1" name="" text="Los hombros de Tokiko temblaron mientras dejaba escapar un&#xA;acalorado gemido." voices=""/>
    <msg @l="794" add="true" char_id="-1" frame_id="-1" name="" text=" Su piel desnuda le absorbió las palmas." voices=""/>
    <msg @l="796" char_id="-1" frame_id="-1" name="" text="(Esto es lo que quería ocultar, ¿eh?)." voices=""/>
    <msg @l="798" char_id="-1" frame_id="-1" name="" text="Ya parecían enormes cuando estaba vestida, pero eso no era&#xA;nada comparado con su verdadero tamaño." voices=""/>
    <msg @l="799" add="true" char_id="-1" frame_id="-1" name="" text=" Tal vez la molestaban&#xA;mucho por eso." voices=""/>
    <msg @l="800" char_id="-1" frame_id="-1" name="" page_feed="true" text="Comprendía cómo se sentía, pero también consideraba que era&#xA;un desperdicio no alardear de ellos." voices=""/>
    <page @l="802" fade="300"/>
    <wait_chgmv/>
    <ev @l="803" src="ev0h04ch" time="0"/>
    <msg @l="805" char_id="-1" frame_id="-1" name="" text="“Aaah...”." voices="toki00021"/>
    <msg @l="807" char_id="-1" frame_id="-1" name="" text="Un gemido ahogado se le escapó cuando la punta de un dedo le&#xA;tocó el pezón." voices=""/>
    <msg @l="808" char_id="-1" frame_id="-1" name="" text="Sus muslos empezaron a retorcerse." voices=""/>
    <msg @l="809" add="true" char_id="-1" frame_id="-1" name="" text=" Esa visión atrajo la&#xA;mano libre de Yuusuke hacia su entrepierna." voices=""/>
    <msg @l="810" char_id="-1" frame_id="-1" name="" text="Le frotó la rodilla cubierta de medias y luego subió hacia su&#xA;muslo." voices=""/>
    <msg @l="811" add="true" char_id="-1" frame_id="-1" name="" text=" Su cálida piel se tensó bajo la tela." voices=""/>
    <msg @l="812" char_id="-1" frame_id="-1" name="" page_feed="true" text="En un instante le subió la falda." voices=""/>
    <page @l="814" fade="300"/>
    <wait_chgmv/>
    <ev @l="815" src="ev0h04ce" time="0"/>
    <msg @l="817" char_id="-1" frame_id="-1" name="" text="“¡Aah!”." voices="toki00022"/>
    <msg @l="819" char_id="-1" frame_id="-1" name="" text="Ella iba a retorcerse, pero él le dio un pequeño pellizco en&#xA;el pezón para mantenerla en su sitio." voices=""/>
    <msg @l="820" char_id="-1" frame_id="-1" name="" text=" Lo hizo rodar mientras&#xA;comenzaba a hincharse, y al mismo tiempo deslizó sus dedos&#xA;en la cintura de sus medias." voices="" add="true"/>
    <msg @l="821" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Pasando por encima del caliente&#xA;abdomen, siguió recto dentro de la ropa interior." voices="" add="true"/>
    <page @l="822" fade="300"/>
    <msg @l="823" char_id="-1" frame_id="-1" name="" text="Tokiko apretó las piernas cuando los dedos de Yuusuke pasaron&#xA;por su fino arbusto en busca del verdadero premio." voices=""/>
    <msg @l="824" add="true" char_id="-1" frame_id="-1" name="" text=" Ella&#xA;estaba apretando demasiado para que él pudiera abrirse paso." voices=""/>
    <msg @l="825" char_id="-1" frame_id="-1" name="" text="Tokiko solía ser muy tímida. Verla objetar no hacía más que&#xA;echar leña al fuego de su lujuria." voices=""/>
    <msg @l="826" char_id="-1" frame_id="-1" name="" text=" Mientras un par de dedos&#xA;jugaban con su pubis, el otro estimulaba su pezón." voices="" add="true"/>
    <msg @l="827" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Agarró un&#xA;montículo entero entre sus manos, disfrutando de su peso y&#xA;tamaño mientras apretaba. Y, presionando nuevamente el pezón&#xA;de su pecho." voices="" add="true"/>
    <page @l="829" fade="300"/>
    <wait_chgmv/>
    <ev @l="830" src="ev0h04cd" time="0"/>
    <msg @l="832" char_id="-1" frame_id="-1" name="" text="“Mm... Ngh...”." voices="toki00023"/>
    <msg @l="834" char_id="-1" frame_id="-1" name="" text="Mientras él seguía estimulando el pezón, completamente&#xA;erecto, la tensión en su parte inferior comenzó a aliviarse.&#xA;" voices=""/>
    <msg @l="835" char_id="-1" frame_id="-1" name="" text="Sin embargo, no forzó su mano hacia abajo, sino que prefirió&#xA;seguir haciéndole cosquillas en su vientre mientras le&#xA;acariciaba el pecho." voices="" add="true"/>
    <msg @l="836" char_id="-1" frame_id="-1" name="" page_feed="true" text="Finalmente, su persistencia dio resultado. Sus muslos se&#xA;abrieron, permitiéndole acceder a lo que había debajo." voices=""/>
    <page @l="838" fade="300"/>
    <wait_chgmv/>
    <ev @l="839" effect="cross_fade" src="ev0h04ee" time="800"/>
    <msg @l="841" char_id="-1" frame_id="-1" name="" text="“¡Aaah!”." voices="toki00024"/>
    <msg @l="843" char_id="-1" frame_id="-1" name="" text="El calor y la humedad." voices=""/>
    <msg @l="844" char_id="-1" frame_id="-1" name="" text="La miel goteaba de su hendidura ligeramente abierta." voices=""/>
    <msg @l="846" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Supongo que tú también estás disfrutando”." voices=""/>
    <page @l="848" fade="300"/>
    <wait_chgmv/>
    <ev @l="849" src="ev0h04ed" time="0"/>
    <msg @l="851" char_id="-1" frame_id="-1" name="" text="Susurró. Tokiko suspiró y se dio la vuelta." voices=""/>
    <msg @l="852" char_id="-1" frame_id="-1" name="" text=" La mano de&#xA;Yuusuke estaba entre sus piernas. Ella no se resistía,&#xA;sino que protestaba débilmente." voices="" add="true"/>
    <se @l="854" buf="1" src="hse101" volume="1."/>
    <msg @l="856" char_id="-1" frame_id="-1" name="" text="Pasó sus dedos por los labios inferiores, recogiendo los&#xA;jugos a medida que avanzaba." voices=""/>
    <msg @l="857" add="true" char_id="-1" frame_id="-1" name="" text=" Cada pequeño chillido hacía que&#xA;Tokiko se retorciera y tratara de ocultarlo." voices=""/>
    <se @l="859" buf="1" src="hse102" volume="1."/>
    <msg @l="861" char_id="-1" frame_id="-1" name="" text="Su valiente frente solo estimuló aún más su deseo de&#xA;estimularla. Hizo más fuertes los chillidos, disfrutando de&#xA;lo realmente empapada que estaba." voices=""/>
    <msg @l="862" char_id="-1" frame_id="-1" name="" text="Jugando con los labios inferiores de la chica, presionó con&#xA;la punta de un dedo la apretada fuente del chorro." voices=""/>
    <msg @l="863" char_id="-1" frame_id="-1" name="" text=" Aunque el&#xA;abdomen de Tokiko se tensó visiblemente, sus muslos&#xA;permanecieron abiertos." voices="" add="true"/>
    <msg @l="864" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" No tenía intención de resistirse." voices=""/>
    <page @l="866" fade="300"/>
    <wait_chgmv/>
    <ev @l="867" src="ev0h04ef" time="0"/>
    <se @l="868" buf="1" src="hse103" volume="1."/>
