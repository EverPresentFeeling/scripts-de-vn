    <msg @l="83" char_id="-1" frame_id="-1" name="" text="Yuusuke miró el reloj y luego aclaró su mente." voices=""/>
    <msg @l="84" add="true" char_id="-1" frame_id="-1" name="" text=" Ya era hora&#xA;de que se fuera." voices=""/>
    <msg @l="85" char_id="-1" frame_id="-1" name="" page_feed="true" text="Siempre se acostaba hasta el último momento, lo que le dejaba&#xA;poco tiempo libre por las mañanas." voices=""/>
    <page @l="87" fade="300"/>
    <se @l="88" buf="2" loop="true" src="se059" volume="1."/>
    <se @l="89" buf="1" src="se078" volume="1."/>
    <wait_chgmv/>
    <bg @l="90" alpha="3" src="bg69a" time="800"/>
    <msg @l="92" char_id="-1" frame_id="-1" name="" text="Mientras se vestía, se dio cuenta de que no tenía una muda de&#xA;calcetines, así que salió al balcón." voices=""/>
    <msg @l="93" char_id="-1" frame_id="-1" name="" text=" Ahí era donde colgaba la&#xA;ropa para secar, así que debería haber calcetines allí." voices="" add="true"/>
    <msg @l="94" char_id="-1" frame_id="-1" name="" text="Su apartamento estaba en el quinto piso, lo que le permitía&#xA;una vista amplia e ininterrumpida de los alrededores." voices=""/>
    <msg @l="95" char_id="-1" frame_id="-1" name="" text=" Abajo&#xA;había coches arrastrándose por la carretera, niños yendo a&#xA;la escuela y hombres de negocios yendo al trabajo." voices="" add="true"/>
    <msg @l="96" char_id="-1" frame_id="-1" name="" text="Una escena perfectamente ordinaria y cotidiana." voices=""/>
    <msg @l="97" char_id="-1" frame_id="-1" name="" text=" Banal,&#xA;pero pacífica." voices="" add="true"/>
    <msg @l="99" char_id="-1" frame_id="-1" name="" text="“Bonito día el de hoy”." voices=""/>
    <stopse @l="101" buf="2" fadeout="1000"/>
    <se @l="102" buf="1" src="se068" volume="1."/>
    <wait_chgmv/>
    <bg @l="103" alpha="3" src="bg70a" time="800"/>
    <msg @l="105" char_id="-1" frame_id="-1" name="" page_feed="true" text="Yuusuke volvió a entrar, se puso el traje, tomó su bolsa y se&#xA;fue." voices=""/>
    <se @l="108" buf="1" src="se003" volume="1."/>
    <wait_chgmv/>
    <bg @l="109" effect="cross_fade" src="black" time="1000"/>
