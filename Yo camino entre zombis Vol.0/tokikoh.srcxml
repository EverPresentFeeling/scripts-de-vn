    <msg @l="11" char_id="-1" frame_id="-1" name="" text="Estaba violando a una mujer." voices=""/>
    <msg @l="12" char_id="-1" frame_id="-1" name="" text="No pudo distinguir su rostro." voices=""/>
    <msg @l="13" char_id="-1" frame_id="-1" name="" text="Pero él conocía el tacto de cada centímetro de su cuerpo." voices=""/>
    <msg @l="14" char_id="-1" frame_id="-1" name="" text="Era alguien con quien había tenido sexo innumerables veces." voices=""/>
    <msg @l="15" char_id="-1" frame_id="-1" name="" text="Golpeó sus caderas con fuerza y rapidez, como siempre." voices=""/>
    <msg @l="16" char_id="-1" frame_id="-1" name="" text="No respondió en absoluto." voices=""/>
    <msg @l="17" char_id="-1" frame_id="-1" name="" text="Tenía el control total y absoluto." voices=""/>
    <flash @l="19" count="2" fade="600" image="black"/>
    <msg @l="21" char_id="-1" frame_id="-1" name="" text="Y sin embargo, algo en esto le estimulaba de una manera que&#xA;el sexo normal no podía." voices=""/>
    <msg @l="22" char_id="-1" frame_id="-1" name="" page_feed="true" text="Fue entonces cuando supo que estaba soñando de nuevo." voices=""/>
    <page @l="24" fade="300"/>
