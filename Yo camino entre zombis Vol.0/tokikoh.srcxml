    <wait_chgmv/>
    <bg @l="189" alpha="4" src="bg100a" time="800"/>
    <bgm @l="190" src="bgm04" volume="1."/>
    <msg @l="192" char_id="-1" frame_id="-1" name="" text="La nueva obra que visitó al día siguiente estaba lejos de la&#xA;oficina. Hasta las nueve de la noche no terminó sus&#xA;reuniones y pudo regresar." voices=""/>
    <msg @l="193" char_id="-1" frame_id="-1" name="" text=" La mayoría de la gente ya había&#xA;salido del trabajo y no quedaba nadie en su planta." voices="" add="true"/>
    <msg @l="194" char_id="-1" frame_id="-1" name="" text="Dejó caer su bolsa junto a la silla, apoyó los pies en el&#xA;escritorio y se estiró mucho." voices=""/>
    <msg @l="196" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Uf... estoy agotado. Un breve descanso y luego a casa,&#xA;creo”." voices=""/>
    <page @l="198" fade="300"/>
    <msg @l="200" char_id="-1" frame_id="-1" name="" text="Sacó algo de cambio de su cartera y estaba a punto de&#xA;dirigirse a la máquina expendedora del pasillo, cuando se&#xA;fijó en una mujer que estaba en la entrada." voices=""/>
    <msg @l="201" add="true" char_id="-1" frame_id="-1" name="" text=" Inmediatamente&#xA;se congeló." voices=""/>
    <msg @l="205" char_id="-1" frame_id="-1" name="" text="“...”." voices="toki00000"/>
    <msg @l="207" char_id="-1" frame_id="-1" name="" text="La mujer inclinó la cabeza y entró en el lugar." voices=""/>
    <se @l="209" buf="1" src="se022" volume="1."/>
    <msg @l="211" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Parecía&#xA;que trabajaba para la empresa. Lo primero que pensó fue&#xA;que debía haberse dejado algo." voices=""/>
    <page @l="213" fade="300"/>
    <stopbgm @l="214" fadeout="2000"/>
    <char @l="215" id="l_to" pos="auto" src="l_tok_aajd"/>
    <msg @l="217" char_id="-1" frame_id="-1" name="" text="Sin embargo, cuando estuvo lo suficientemente cerca como para&#xA;que pudiera ver su rostro, jadeó." voices=""/>
    <msg @l="219" char_id="-1" frame_id="-1" name="" text="“Oh...”." voices=""/>
    <msg @l="221" char_id="-1" frame_id="-1" name="" text="Estaba seguro de saber quién era." voices=""/>
    <bgm @l="223" src="bgm03" volume="1."/>
    <msg @l="225" add="true" char_id="-1" frame_id="-1" name="" text=" Podía sentirlo en sus&#xA;huesos." voices=""/>
