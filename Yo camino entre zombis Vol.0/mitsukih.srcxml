    <msg @l="1157" char_id="-1" frame_id="-1" name="" text="Y era el mayor placer que había conocido en toda su vida." voices=""/>
    <msg @l="1158" char_id="-1" frame_id="-1" name="" text="La vagina de Mitsuki siguió apretando el pene de Yuusuke." voices=""/>
    <msg @l="1159" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Lo&#xA;guio más adentro, instándolo a liberar hasta la última gota&#xA;dentro de ella." voices="" add="true"/>
    <page @l="1161" fade="300"/>
    <stopse @l="1162" buf="1" fadeout="1000"/>
    <msg @l="1164" char_id="-1" frame_id="-1" name="" text="Por fin, su pene se retiró de la funda." voices=""/>
    <msg @l="1165" add="true" char_id="-1" frame_id="-1" name="" text=" Un revoltijo de jugos&#xA;salpicó el suelo." voices=""/>
    <wait_chgmv/>
    <bg @l="1167" src="black" time="1000"/>
    <msg @l="1169" char_id="-1" frame_id="-1" name="" text="“Ah...”." voices="mitu00932"/>
    <msg @l="1171" char_id="-1" frame_id="-1" name="" text="La fuerza de Mitsuki se desvaneció." voices=""/>
    <se @l="1173" buf="1" src="se017" volume="1."/>
    <msg @l="1175" char_id="-1" frame_id="-1" name="" text=" Justo cuando estaba&#xA;a punto de derrumbarse, Yuusuke la apoyó." voices="" add="true"/>
    <msg @l="1176" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Le acarició&#xA;suavemente la cabeza mientras le susurraba algo que ella no&#xA;captó." voices=""/>
    <page @l="1178" fade="300"/>
    <msg @l="1180" char_id="-1" frame_id="-1" name="" text="“Aaah... Aaah... Uff...”." voices="mitu00933"/>
    <msg @l="1182" char_id="-1" frame_id="-1" name="" text="Mitsuki jadeó para recuperar el aliento." voices=""/>
    <msg @l="1183" char_id="-1" frame_id="-1" name="" text=" Cuando la emoción&#xA;se desvaneció, Yuusuke suspiró aliviado." voices="" add="true"/>
    <stopbgm @l="1185" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="1186" effect="cross_fade" src="bg98a" time="1000"/>
    <msg @l="1188" char_id="-1" frame_id="-1" name="" text="“Perfecto, me siento muy bien ahora. Voy a hacer muchas cosas&#xA;hoy”." voices=""/>
    <msg @l="1190" char_id="-1" frame_id="-1" name="" text="Sonrió sin miedo." voices=""/>
    <msg @l="1191" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Mientras tanto, Mitsuki agachó la cabeza&#xA;tratando de recomponerse mientras decía..." voices="" add="true"/>
    <page @l="1193" fade="300"/>
    <char @l="1194" id="mit_" pos="auto" src="mit_aaer"/>
    <msg @l="1196" char_id="-1" frame_id="-1" name="" text="“Usted y yo debemos tener una charla”." voices="mitu00934"/>
    <msg @l="1197" char_id="-1" frame_id="-1" name="" text="“¿Eh?”." voices=""/>
    <begin_trans @l="1199" time="200"/>
    <clear @l="1200" class="char"/>
    <char @l="1201" id="l_mi" pos="auto" src="l_mit_aaec"/>
    <end_trans @l="1202"/>
    <msg @l="1204" char_id="-1" frame_id="-1" name="" text="“¡Oye! ¡Necesita hacer esto bien!”." voices="mitu00935"/>
    <msg @l="1205" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Oh. Uh, de acuerdo...”." voices=""/>
    <page @l="1207" fade="300"/>
    <stopbgm @l="1208" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="1209" effect="cross_fade" src="white" time="1000"/>
    <se @l="1210" buf="1" src="se189" volume="1."/>
    <wait_chgmv/>
    <bg @l="1211" effect="cross_fade" src="bg69a" time="1200"/>
    <waitse @l="1212" buf="1"/>
    <msg @l="1214" char_id="-1" frame_id="-1" name="" text="Así que Yuusuke hizo exactamente lo que le dijo, y los dos se&#xA;convirtieron en pareja." voices=""/>
    <se @l="1216" buf="1" src="se190" volume="1."/>
    <msg @l="1218" char_id="-1" frame_id="-1" name="" text="En poco tiempo se presentó a los padres de Mitsuki, que al&#xA;principio parecían dudar de todo el asunto. Sin embargo,&#xA;pocos años después, los dos se casaron." voices=""/>
    <msg @l="1219" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Mitsuki gobernaba su&#xA;casa con mano de hierro, manteniendo a Yuusuke a raya, y los&#xA;dos criaron juntos una familia feliz." voices="" add="true"/>
    <page @l="1221" fade="300"/>
    <wait_chgmv/>
    <bg @l="1222" src="white" time="2000"/>
    <waitse @l="1223" buf="1"/>
    <end_scene @l="1226"/>
    <wait @l="1230" time="1000"/>
    <bgm @l="1231" src="bgm04" volume="1."/>
    <wait_chgmv/>
    <bg @l="1232" alpha="1" src="bg76a" time="800"/>
    <wait_chgmv/>
    <bg @l="1233" src="bg79a" time="1000"/>
    <char @l="1234" id="tmr_" pos="auto" src="tmr_aabk"/>
    <shake @l="1235" amp="5" class="char" cycle="1" duration="200" type="random"/>
    <msg @l="1237" char_id="-1" frame_id="-1" name="" text="“¿Eh? ¿Por qué estoy temblando?”." voices=""/>
    <msg @l="1239" char_id="-1" frame_id="-1" name="" text="Para cuando Yuusuke regresó al supermercado de su recorrido&#xA;de suministros, ya casi había amanecido." voices=""/>
    <char @l="1241" id="tmr_" pos="auto" src="tmr_aabc"/>
    <msg @l="1243" char_id="-1" frame_id="-1" name="" text="“¿Me estoy poniendo enfermo? Tengo que tener cuidado. Aunque&#xA;es demasiado tarde para preocuparse por esa mierda ahora”." voices=""/>
    <clear @l="1245" class="char"/>
    <wait_chgmv/>
    <bg @l="1246" alpha="3" src="black" time="800"/>
    <msg @l="1248" char_id="-1" frame_id="-1" name="" text="Ahogó un bostezo mientras se dirigía a los vestuarios de la&#xA;tercera planta, donde dormía." voices=""/>
    <msg @l="1249" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Después de haber pasado toda la&#xA;noche corriendo de un lado a otro, su agotamiento era&#xA;máximo." voices="" add="true"/>
    <page @l="1251" fade="300"/>
    <se @l="1252" buf="1" src="se003" volume="1."/>
    <wait_chgmv/>
    <bg @l="1253" alpha="3" src="bg84a" time="800"/>
    <se @l="1254" buf="1" src="se004" volume="1."/>
    <msg @l="1256" char_id="-1" frame_id="-1" name="" text="Cuando entró en la habitación, se quedó helado." voices=""/>
    <msg @l="1258" char_id="-1" frame_id="-1" name="" text="“¿Por qué duerme aquí?”." voices=""/>
    <msg @l="1260" char_id="-1" frame_id="-1" name="" text="Mitsuki estaba envuelta bajo la manta, durmiendo&#xA;plácidamente." voices=""/>
    <msg @l="1261" char_id="-1" frame_id="-1" name="" text="Tenía una sonrisa bobalicona en la cara. Tal vez estaba&#xA;teniendo un sueño agradable." voices=""/>
    <msg @l="1263" char_id="-1" frame_id="-1" name="" text="“Vamos, despierta”." voices=""/>
    <se @l="1265" buf="1" src="se049" volume="1."/>
    <shake @l="1266" amp="10" class="bg" cycle="1" duration="200" type="random"/>
    <msg @l="1268" char_id="-1" frame_id="-1" name="" text="Le sacudió suavemente los hombros." voices=""/>
    <msg @l="1269" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Mitsuki lo miró con ojos&#xA;soñolientos." voices=""/>
    <page @l="1271" fade="300"/>
    <char @l="1272" id="l_mi" pos="auto" src="l_mit_aabs"/>
    <msg @l="1274" char_id="-1" frame_id="-1" name="" text="“Ah... Yuusuke-san”." voices="mitu00936"/>
    <msg @l="1276" char_id="-1" frame_id="-1" name="" text="“Ve a dormir a tu propia habitación”." voices=""/>
    <char @l="1278" id="l_mi" pos="auto" src="l_mit_aabf"/>
    <msg @l="1280" char_id="-1" frame_id="-1" name="" text="“¿Nngh? Je, je, je... Hoy llegaste temprano a casa”." voices="mitu00937"/>
    <clear @l="1282" class="char"/>
    <shake @l="1283" amp="20" class="bg" cycle="1" duration="200" type="random"/>
    <msg @l="1285" char_id="-1" frame_id="-1" name="" text="Mitsuki lo acercó de repente..." voices=""/>
    <msg @l="1286" add="true" char_id="-1" frame_id="-1" name="" text=" y se acurrucó en su pecho." voices=""/>
    <msg @l="1287" char_id="-1" frame_id="-1" name="" text="Yuusuke estaba en total shock." voices=""/>
    <msg @l="1289" char_id="-1" frame_id="-1" name="" page_feed="true" text="“O-Oye, ¿todavía estás medio dormida? ¡Despierta, chica!”." voices=""/>
    <page @l="1291" fade="300"/>
    <char @l="1292" id="mit_" pos="auto" src="mit_aabp"/>
    <msg @l="1294" char_id="-1" frame_id="-1" name="" text="Mitsuki miró a la habitación sin comprender." voices=""/>
    <msg @l="1295" char_id="-1" frame_id="-1" name="" text="Y finalmente, se dio cuenta de dónde había estado durmiendo." voices=""/>
    <char @l="1297" id="mit_" pos="auto" src="mit_aabl"/>
    <msg @l="1299" char_id="-1" frame_id="-1" name="" text="Sus ojos se abrieron lentamente." voices=""/>
    <msg @l="1300" char_id="-1" frame_id="-1" name="" text="Entonces, empezaron a dar vueltas en pánico." voices=""/>
    <msg @l="1301" char_id="-1" frame_id="-1" name="" text="Ella sabía que él lo sabía." voices=""/>
    <msg @l="1302" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Su rostro se volvió escarlata." voices=""/>
    <page @l="1304" fade="300"/>
    <char @l="1305" id="mit_" pos="auto" src="mit_aabi"/>
    <shake @l="1306" amp="10" class="char" cycle="2" duration="200" type="random"/>
    <msg @l="1307" char_id="-1" frame_id="-1" name="" text="“¡Perdón!”." voices="mitu00938"/>
    <clear @l="1309" class="char"/>
    <se @l="1310" buf="1" src="se003" volume="1."/>
    <waitse @l="1311" buf="1"/>
    <se @l="1312" buf="1" src="se004" volume="1."/>
    <waitse @l="1313" buf="1"/>
    <se @l="1314" buf="1" src="se023" volume="1."/>
    <msg @l="1316" char_id="-1" frame_id="-1" name="" text="“¡O-Oye!”." voices=""/>
    <msg @l="1318" char_id="-1" frame_id="-1" name="" page_feed="true" text="Se zafó apresuradamente de sus brazos, se levantó y salió&#xA;corriendo por la puerta." voices=""/>
    <stopse @l="1320" buf="1"/>
    <page @l="1321" fade="300"/>
    <msg @l="1323" char_id="-1" frame_id="-1" name="" text="“¿Qué diablos fue eso? Dios, es rara”." voices=""/>
    <msg @l="1325" char_id="-1" frame_id="-1" name="" text="Yuusuke sacudió la cabeza y miró alrededor de la habitación." voices=""/>
    <msg @l="1326" char_id="-1" frame_id="-1" name="" text="A pesar de la curiosidad que sentía por el comportamiento de&#xA;Mitsuki, estaba demasiado cansado para ocuparse de algo en&#xA;ese momento." voices=""/>
    <msg @l="1327" add="true" char_id="-1" frame_id="-1" name="" text=" Así que, simplemente se deslizó bajo la manta&#xA;que acababa de dejar libre." voices=""/>
    <wait_chgmv/>
    <bg @l="1329" src="black" time="2000"/>
    <msg @l="1331" char_id="-1" frame_id="-1" name="" page_feed="true" text="Todavía estaba llena de su calor, y Yuusuke se quedó dormido&#xA;en un instante." voices=""/>
    <page @l="1333" fade="300"/>
    <stopse @l="1334" buf="1" fadeout="1000"/>
    <stopbgm @l="1335" fadeout="2000"/>
    <wait @l="1336" time="2000"/>
  </code>
</xscript>