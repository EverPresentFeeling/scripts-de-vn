<?xml version="1.0" encoding="utf-8"?>
<xscript>
  <debug editor="&quot;C:\Program Files (x86)\sakura\sakura.exe&quot; &quot;%src%&quot; -Y=%line%" src="D:\zombies\Series Build\開発データ\開発環境\vol別\vol0\script\text\深月・妄想h.txt"/>
  <code>
    <description @l="1" text="深月・妄想Ｈ"/>
    <se @l="4" buf="1" src="se142" volume="1."/>
    <wait_chgmv/>
    <bg @l="5" src="bg86d" time="1500"/>
    <msg @l="7" char_id="-1" frame_id="-1" name="" text="A Mitsuki le resultaba inusualmente difícil dormir esa noche." voices=""/>
    <msg @l="8" char_id="-1" frame_id="-1" name="" text="Estaba tumbada en su cama de la oficina del supermercado en&#xA;un medio sueño superficial; en algún lugar entre la&#xA;consciencia y el mundo de los sueños." voices=""/>
    <stopse @l="10" buf="1" fadeout="2000"/>
    <msg @l="12" char_id="-1" frame_id="-1" name="" text="“Ngh...”." voices="mitu00800"/>
    <msg @l="14" char_id="-1" frame_id="-1" name="" text="Justo en ese momento la despertó por completo un fuerte ruido&#xA;en la distancia." voices=""/>
    <se @l="16" buf="1" src="se049" volume="1."/>
    <char @l="17" id="l_mi" pos="auto" src="l_mit_aabj"/>
    <msg @l="19" char_id="-1" frame_id="-1" name="" text="(¿Qué fue eso?)." voices="mitu00801"/>
    <msg @l="21" char_id="-1" frame_id="-1" name="" text="Contuvo la respiración y aguzó las orejas." voices=""/>
    <msg @l="22" char_id="-1" frame_id="-1" name="" page_feed="true" text="Pero lo único que podía oír era la respiración de sus&#xA;hermanos mientras dormían a su lado." voices=""/>
    <stopse @l="24" buf="1"/>
    <page @l="25" fade="300"/>
    <clear @l="26" class="char"/>
    <char @l="27" id="mit_" pos="auto" src="mit_aabp"/>
    <bgm @l="28" src="bgm03" volume="1."/>
    <msg @l="30" char_id="-1" frame_id="-1" name="" text="(¿Lo he imaginado? ¿O lo he soñado?)." voices="mitu00802"/>
    <msg @l="32" char_id="-1" frame_id="-1" name="" text="El hecho de no saberlo la ponía nerviosa." voices=""/>
    <msg @l="33" char_id="-1" frame_id="-1" name="" text="Esperaba que fuera un sueño, pero si fuera real..." voices=""/>
    <msg @l="34" add="true" char_id="-1" frame_id="-1" name="" text=" Si se&#xA;trataba de su imaginación, podría ocurrir algo terrible." voices=""/>
    <char @l="36" id="mit_" pos="auto" src="mit_aabj"/>
    <msg @l="38" char_id="-1" frame_id="-1" name="" text="(Si es un zombi o un intruso...)." voices="mitu00803"/>
    <msg @l="40" char_id="-1" frame_id="-1" name="" text="Yuusuke se había marchado varios días antes en una excursión&#xA;en busca de provisiones, y no había regresado." voices=""/>
    <msg @l="41" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Mitsuki era la&#xA;única que podía proteger a sus jóvenes hermanos." voices=""/>
    <page @l="43" fade="300"/>
    <char @l="44" id="mit_" pos="auto" src="mit_aaba"/>
    <msg @l="46" char_id="-1" frame_id="-1" name="" text="(Voy a ir a echar un vistazo)." voices="mitu00804"/>
    <msg @l="48" char_id="-1" frame_id="-1" name="" text="Con toda probabilidad no era nada de lo que preocuparse, pero&#xA;la ansiedad no la dejaba dormir." voices=""/>
    <msg @l="49" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Salió sigilosamente de la&#xA;habitación, haciendo lo posible por no despertar a sus&#xA;hermanos, mientras intentaba calmar sus nervios." voices="" add="true"/>
    <clear @l="51" class="char"/>
    <page @l="57" fade="300"/>
    <se @l="58" buf="1" src="se003" volume="1."/>
    <wait_chgmv/>
    <bg @l="59" alpha="3" src="bg79d" time="800"/>
    <msg @l="61" char_id="-1" frame_id="-1" name="" text="(Está helado)." voices="mitu00805"/>
    <msg @l="63" char_id="-1" frame_id="-1" name="" text="La calefacción estaba encendida, pero el techo era tan alto&#xA;que todo el aire caliente subía y dejaba el suelo frío." voices=""/>
    <msg @l="64" char_id="-1" frame_id="-1" name="" text="&#xA;Avanzó hacia delante, iluminando cautelosamente las sombras&#xA;con su linterna." voices="" add="true"/>
    <msg @l="65" char_id="-1" frame_id="-1" name="" text="No había nada." voices=""/>
    <msg @l="66" char_id="-1" frame_id="-1" name="" text="Ni siquiera tenía una idea aproximada de la dirección de la&#xA;que procedía el sonido." voices=""/>
    <msg @l="67" char_id="-1" frame_id="-1" name="" page_feed="true" text=" De hecho, no estaba segura de haberlo&#xA;escuchado en primer lugar." voices="" add="true"/>
    <page @l="69" fade="300"/>
    <char @l="70" id="mit_" pos="auto" src="mit_aabi"/>
    <msg @l="72" char_id="-1" frame_id="-1" name="" text="(...)." voices="mitu00806"/>
    <msg @l="74" char_id="-1" frame_id="-1" name="" text="Su entorno había vuelto a estar en un silencio sepulcral." voices=""/>
    <clear @l="76" class="char"/>
    <se @l="78" buf="1" src="se109" volume="1."/>
    <msg @l="79" char_id="-1" frame_id="-1" name="" text="&#xA;El sonido de sus propios pasos era una cacofonía que le&#xA;hacía apretar el pecho." voices="" add="true"/>
    <stopse @l="81" buf="1"/>
    <msg @l="83" char_id="-1" frame_id="-1" name="" text="Incapaz de aguantar más, se quedó paralizada." voices=""/>
    <msg @l="85" char_id="-1" frame_id="-1" name="" text="(¿Siempre se sintió tan vacío este lugar?)." voices="mitu00807"/>
    <msg @l="87" char_id="-1" frame_id="-1" name="" text="Era completamente diferente a como se sentía en el día." voices=""/>
    <msg @l="88" char_id="-1" frame_id="-1" name="" text="Sus hermanos estaban durmiendo en la oficina." voices=""/>
    <msg @l="89" add="true" char_id="-1" frame_id="-1" name="" text=" Era como&#xA;si fuera la única persona que quedaba en todo el mundo." voices=""/>
    <msg @l="90" char_id="-1" frame_id="-1" name="" text="Se abrazó a sí misma para intentar detener el temblor." voices=""/>
    <msg @l="91" char_id="-1" frame_id="-1" name="" text="¿Quizá había tenido una pesadilla antes de despertarse y no&#xA;la recordaba?" voices=""/>
    <msg @l="92" char_id="-1" frame_id="-1" name="" text=" Sin importar la razón, se dio cuenta de que&#xA;estaba terriblemente nerviosa." voices="" add="true"/>
    <msg @l="93" char_id="-1" frame_id="-1" name="" page_feed="true" text="Saber que no había nadie que viniera a salvarla la hacía&#xA;sentir muy impotente." voices=""/>
    <page @l="95" fade="300"/>
    <msg @l="97" char_id="-1" frame_id="-1" name="" text="Antes de darse cuenta, sus pies la llevaron al vestuario,&#xA;donde Yuusuke solía dormir." voices=""/>
    <msg @l="99" char_id="-1" frame_id="-1" name="" text="(Tal vez ese sonido era el regreso de Takemura)." voices="mitu00808"/>
    <wait_chgmv/>
    <bg @l="101" alpha="3" src="bg84a" time="800"/>
    <msg @l="103" char_id="-1" frame_id="-1" name="" text="Esa efímera esperanza pronto se desvaneció." voices=""/>
    <msg @l="104" char_id="-1" frame_id="-1" name="" text=" El vestuario&#xA;estaba vacío, con la manta de Yuusuke hecha un ovillo en el&#xA;suelo." voices="" add="true"/>
    <char @l="106" id="mit_" pos="auto" src="mit_aabs"/>
    <msg @l="108" char_id="-1" frame_id="-1" name="" text="(No está aquí)." voices="mitu00809"/>
    <msg @l="110" char_id="-1" frame_id="-1" name="" text="Sintiéndose aún más impotente, decidió doblar cuidadosamente&#xA;la manta." voices=""/>
    <msg @l="112" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Al hacerlo, un pequeño trozo de tela cayó al suelo." voices="" add="true"/>
    <page @l="114" fade="300"/>
    <char @l="115" id="mit_" pos="auto" src="mit_aabb"/>
    <msg @l="117" char_id="-1" frame_id="-1" name="" text="(¡Oh!)." voices="mitu00810"/>
    <msg @l="119" char_id="-1" frame_id="-1" name="" text="No había duda, eran las bragas de Mitsuki." voices=""/>
    <msg @l="120" char_id="-1" frame_id="-1" name="" text=" Se las había&#xA;quitado para tener sexo, y luego se había olvidado de ellas&#xA;y las había dejado atrás." voices="" add="true"/>
    <msg @l="121" add="true" char_id="-1" frame_id="-1" name="" text=" Se apresuró a recogerlas." voices=""/>
    <char @l="123" id="mit_" pos="auto" src="mit_aabp"/>
    <msg @l="125" char_id="-1" frame_id="-1" name="" text="(Podría haberme dicho, al menos)." voices="mitu00811"/>
    <msg @l="127" char_id="-1" frame_id="-1" name="" text="Su cara ardía de vergüenza, pero era posible que Yuusuke&#xA;simplemente no se hubiera dado cuenta." voices=""/>
    <msg @l="128" char_id="-1" frame_id="-1" name="" text=" Yuusuke no parecía&#xA;tener mucho interés en la ropa interior, así que no era&#xA;probable que la hubiera guardado a propósito." voices="" add="true"/>
    <msg @l="129" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Mientras&#xA;pensaba en eso, su mente comenzó a girar hacia sus muchos&#xA;encuentros en esta habitación." voices="" add="true"/>
    <stopbgm @l="131" fadeout="2000"/>
    <page @l="132" fade="300"/>
    <msg @l="134" char_id="-1" frame_id="-1" name="" text="(¿Cuántas veces lo hemos hecho aquí ya?)." voices="mitu00812"/>
    <se @l="136" buf="1" src="se049" volume="1."/>
    <clear @l="137" layer="mit_"/>
    <msg @l="139" char_id="-1" frame_id="-1" name="" text="Entonces, mientras doblaba la manta, una dulce tentación la&#xA;invadió lentamente." voices=""/>
    <msg @l="140" char_id="-1" frame_id="-1" name="" page_feed="true" text="Así que la colocó y se acurrucó sobre ella." voices=""/>
    <wait_chgmv/>
    <bg @l="142" alpha="2" src="black" time="800"/>
    <bgm @l="143" src="bgm09" volume="1."/>
    <stopse @l="144" buf="1"/>
    <page @l="145" fade="300"/>
    <msg @l="147" char_id="-1" frame_id="-1" name="" text="(Huele a... Takemura-san)." voices="mitu00813"/>
    <msg @l="149" char_id="-1" frame_id="-1" name="" text="Sabía que había algo sucio en lo que estaba haciendo, pero&#xA;era extrañamente tranquilizador." voices=""/>
    <msg @l="150" char_id="-1" frame_id="-1" name="" text="“Espero que vuelva pronto”, se encontró pensando." voices=""/>
    <msg @l="151" char_id="-1" frame_id="-1" name="" text=" Por mucho&#xA;que odiara admitirlo, era más dependiente de él de lo que se&#xA;había dado cuenta." voices="" add="true"/>
    <msg @l="152" char_id="-1" frame_id="-1" name="" text=" En ese momento, la falta de alguien en&#xA;quien apoyarse hizo que el agujero solitario en su corazón&#xA;fuera aún más grande." voices="" add="true"/>
    <msg @l="153" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Deseaba que él estuviera con ella en&#xA;ese momento, aunque lo único que quisiera fuera sexo." voices="" add="true"/>
    <page @l="155" fade="300"/>
    <msg @l="156" char_id="-1" frame_id="-1" name="" text="Mientras pensaba en Yuusuke, se encontró cerrando los ojos y&#xA;moviendo las manos." voices=""/>
    <msg @l="157" char_id="-1" frame_id="-1" name="" text="Nadie entraría en ella." voices=""/>
    <se @l="159" buf="1" src="se049" volume="1."/>
    <msg @l="161" char_id="-1" frame_id="-1" name="" text="“Mmm...”." voices="mitu00814"/>
    <msg @l="163" char_id="-1" frame_id="-1" name="" text="Jugando con su pecho y su entrepierna, trató de imitar la&#xA;forma en que él la tocaba." voices=""/>
    <msg @l="165" char_id="-1" frame_id="-1" name="" text="“Mm... Ngh...”." voices="mitu00815"/>
    <msg @l="167" char_id="-1" frame_id="-1" name="" page_feed="true" text="Pero por mucho que lo intentara, no podía producir el placer&#xA;que esperaba." voices=""/>
    <page @l="169" fade="300"/>
    <stopse @l="170" buf="1"/>
    <wait_chgmv/>
    <bg @l="171" alpha="1" src="bg84a" time="800"/>
    <wait @l="172" time="500"/>
    <msg @l="174" char_id="-1" frame_id="-1" name="" text="“¿Qué estoy haciendo?”." voices="mitu00816"/>
    <msg @l="176" char_id="-1" frame_id="-1" name="" text="Su breve chispa de pasión se enfrió y se rindió." voices=""/>
    <wait_chgmv/>
    <bg @l="178" effect="cross_fade" src="black" time="1000"/>
    <msg @l="180" add="true" char_id="-1" frame_id="-1" name="" text=" Volvió a&#xA;cerrar los ojos y pensó en algo diferente." voices=""/>
    <msg @l="181" char_id="-1" frame_id="-1" name="" text="Recreando aquellos días de paz antes de la pandemia." voices=""/>
    <msg @l="183" char_id="-1" frame_id="-1" name="" text="(Me levantaba por la mañana e iba a clase. Nunca pensé en&#xA;ello en ese momento, pero era muy tranquilo)." voices="mitu00817"/>
    <msg @l="185" char_id="-1" frame_id="-1" name="" text="Mitsuki sonrió débilmente." voices=""/>
    <msg @l="186" add="true" char_id="-1" frame_id="-1" name="" text=" Empezó a quedarse dormida mientras&#xA;pensaba en el pasado." voices=""/>
    <msg @l="187" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Y pronto, se quedó profundamente&#xA;dormida." voices=""/>
    <stopse @l="189" buf="1" fadeout="1000"/>
    <stopbgm @l="190" fadeout="2000"/>
    <wait_chgmv/>
    <bg @l="191" effect="cross_fade" src="white" time="3000"/>
    <page @l="192" fade="300"/>
    <wait @l="194" time="1000"/>
    <wait_chgmv/>
    <bg @l="195" effect="cross_fade" src="bg98a" time="2000"/>
    <wait_chgmv/>
    <bg @l="196" effect="shading_off" src="bg98a" time="1500"/>
    <se @l="197" buf="2" loop="true" src="se082" volume="1."/>
    <msg @l="202" char_id="-1" frame_id="-1" name="" text="Una sacudida debajo de sus pies hizo que su estómago se&#xA;tambaleara." voices=""/>
    <msg @l="203" char_id="-1" frame_id="-1" name="" text=" Cuando abrió los ojos, se dio cuenta de que&#xA;estaba en un autobús." voices="" add="true"/>
    <char @l="205" id="mit_" pos="auto" src="mit_aaee"/>
    <msg @l="207" char_id="-1" frame_id="-1" name="" text="(¿...?)." voices="mitu00818"/>
    <msg @l="209" char_id="-1" frame_id="-1" name="" text="Por un momento no tuvo ni idea de lo que estaba pasando." voices=""/>
    <msg @l="210" char_id="-1" frame_id="-1" name="" text="Entonces, lo recordó." voices=""/>
    <msg @l="211" add="true" char_id="-1" frame_id="-1" name="" text=" Iba en el autobús a clase." voices=""/>
    <msg @l="212" char_id="-1" frame_id="-1" name="" text="Debió haberse quedado dormida." voices=""/>
    <msg @l="213" char_id="-1" frame_id="-1" name="" text="El paisaje por la ventana era el mismo que el de cualquier&#xA;otro día." voices=""/>
    <msg @l="214" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Nada de eso le parecía extraño." voices=""/>
    <page @l="216" fade="300"/>
    <char @l="217" id="mit_" pos="auto" src="mit_aaej"/>
    <msg @l="219" char_id="-1" frame_id="-1" name="" text="Mitsuki miró alrededor del autobús y suspiró." voices=""/>
    <msg @l="221" char_id="-1" frame_id="-1" name="" text="(Seguro que está lleno de gente)." voices="mitu00819"/>
    <clear @l="223" class="char"/>
    <se @l="224" buf="1" src="se149" volume="1."/>
    <msg @l="226" char_id="-1" frame_id="-1" name="" text="Unas nubes oscuras cubrían el cielo y una lluvia torrencial&#xA;golpeaba las calles." voices=""/>
    <msg @l="227" char_id="-1" frame_id="-1" name="" text="El autobús estaba lleno de gente, con tanto calor y humedad&#xA;que parecía que se estaba ahogando." voices=""/>
    <msg @l="228" char_id="-1" frame_id="-1" name="" text=" Debido a la lluvia, mucha&#xA;gente había optado por ir al trabajo en coche, dejando las&#xA;carreteras tan congestionadas que el autobús no avanzaba&#xA;mucho." voices="" add="true"/>
    <msg @l="229" char_id="-1" frame_id="-1" name="" text="Otro día aburrido." voices=""/>
    <msg @l="231" char_id="-1" frame_id="-1" name="" text="(Espero que lleguemos pronto)." voices="mitu00820"/>
    <msg @l="233" char_id="-1" frame_id="-1" name="" page_feed="true" text="Pensó para sí misma mientras la multitud de humanos se movía&#xA;detrás de ella." voices=""/>
    <page @l="235" fade="300"/>
    <wait_chgmv/>
    <ev @l="236" src="cut02aa" time="1000"/>
    <stopse @l="237" buf="1" fadeout="2000"/>
    <bgm @l="238" src="bgm04" volume="1."/>
    <msg @l="240" char_id="-1" frame_id="-1" name="" text="“Hola”." voices=""/>
    <msg @l="242" char_id="-1" frame_id="-1" name="" text="“Oh, Takemura-san”." voices="mitu00821"/>
    <wait_chgmv/>
    <bg @l="244" src="bg98a" time="1000"/>
    <msg @l="246" char_id="-1" frame_id="-1" name="" text="Yuusuke estaba de pie detrás de ella, con un traje." voices=""/>
    <char @l="248" id="mit_" pos="auto" src="mit_aaef"/>
    <msg @l="250" char_id="-1" frame_id="-1" name="" text="“Buenos días. ¿Va de camino al trabajo?”." voices="mitu00822"/>
    <msg @l="251" char_id="-1" frame_id="-1" name="" text="“Si alguna vez llego allí. Este tráfico es una mierda”." voices=""/>
    <msg @l="252" char_id="-1" frame_id="-1" name="" text="“Parece que la vida laboral es estresante”." voices="mitu00823"/>
    <msg @l="253" char_id="-1" frame_id="-1" name="" page_feed="true" text="“Estudia mucho y pronto lo descubrirás por ti misma”." voices=""/>
    <page @l="255" fade="300"/>
    <clear @l="256" class="char"/>
    <scrnfilter @l="257" src="sepia" time="1000"/>
    <msg @l="259" char_id="-1" frame_id="-1" name="" text="Mientras charlaban, Mitsuki recordó cómo había conocido a&#xA;Yuusuke." voices=""/>
    <msg @l="260" char_id="-1" frame_id="-1" name="" text="Había sido varios meses antes." voices=""/>
    <msg @l="262" char_id="-1" frame_id="-1" name="" text="Los dos siempre habían viajado a la misma hora, por lo que&#xA;acababan en el mismo autobús con bastante frecuencia." voices=""/>
    <msg @l="263" char_id="-1" frame_id="-1" name="" text="&#xA;Entonces, un día..." voices="" add="true"/>
    <msg @l="265" char_id="-1" frame_id="-1" name="" page_feed="true" text="(Los dos ayudamos a un anciano que se desmayó en el autobús)." voices="mitu00824"/>
    <page @l="267" fade="300"/>
    <begin_trans @l="268" time="1000"/>
    <scrnfilter @l="269" src=""/>
    <end_trans @l="270"/>
    <msg @l="272" char_id="-1" frame_id="-1" name="" text="Después de eso, empezaron a hablar." voices=""/>
    <msg @l="273" char_id="-1" frame_id="-1" name="" text="Las primeras veces no fue más que intercambiar un “buenos&#xA;días”, pero Mitsuki acabó enamorándose del aura fría y&#xA;madura de Yuusuke." voices=""/>
    <msg @l="274" char_id="-1" frame_id="-1" name="" text="Día a día sus conversaciones se fueron alargando, hasta que&#xA;acabaron intercambiando números." voices=""/>
    <msg @l="275" add="true" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Eso había sido hace una&#xA;semana." voices=""/>
    <page @l="277" fade="300"/>
    <char @l="278" id="mit_" pos="auto" src="mit_aaeq"/>
    <msg @l="280" char_id="-1" frame_id="-1" name="" text="(Si nos acercamos...)." voices="mitu00825"/>
    <msg @l="282" char_id="-1" frame_id="-1" name="" text="Tal vez ella le diga lo que siente." voices=""/>
    <msg @l="283" add="true" char_id="-1" frame_id="-1" name="" text=" Ya había conseguido&#xA;confirmar indirectamente que no tenía novia." voices=""/>
    <msg @l="284" char_id="-1" frame_id="-1" name="" page_feed="true" text="Mitsuki estaba nerviosa por dentro, pero se las arregló para&#xA;continuar la conversación sin dejar que se notara." voices=""/>
    <page @l="286" fade="300"/>
    <char @l="287" id="mit_" pos="auto" src="mit_aaek"/>
    <msg @l="289" char_id="-1" frame_id="-1" name="" text="“Seguro que no estamos llegando a ninguna parte, ¿verdad?”." voices="mitu00826"/>
    <msg @l="290" char_id="-1" frame_id="-1" name="" text="“Quizá debería haber tomado el siguiente”." voices=""/>
    <msg @l="292" char_id="-1" frame_id="-1" name="" text="La humedad del interior del autobús estaba afectando a&#xA;Yuusuke, así que suspiró y se aflojó la corbata." voices=""/>
    <msg @l="293" char_id="-1" frame_id="-1" name="" text="Mitsuki lo observó, paralizada." voices=""/>
    <char @l="295" id="mit_" pos="auto" src="mit_aaeq"/>
    <msg @l="297" char_id="-1" frame_id="-1" name="" text="(Cielos, es guapo)." voices="mitu00827"/>
    <msg @l="299" char_id="-1" frame_id="-1" name="" text="Ella descubrió que sus ojos seguían inconscientemente cada&#xA;pequeño movimiento que él hacía." voices=""/>
    <msg @l="300" char_id="-1" frame_id="-1" name="" page_feed="true" text=" Yuusuke parecía no darse&#xA;cuenta y empezó a murmurar para sí mismo; estaba harto." voices="" add="true"/>
    <page @l="302" fade="300"/>
    <msg @l="304" char_id="-1" frame_id="-1" name="" text="“No quiero ir a trabajar”." voices=""/>
    <msg @l="306" char_id="-1" frame_id="-1" name="" text="De hecho, esa mañana no tenía muy buen aspecto. El cansancio&#xA;estaba escrito en su cara." voices=""/>
    <msg @l="307" add="true" char_id="-1" frame_id="-1" name="" text=" Llevaba un tiempo así." voices=""/>
    <msg @l="308" char_id="-1" frame_id="-1" name="" text="La falta crónica de sueño, supuso, había hecho que incluso su&#xA;expresión habitualmente dura perdiera su brillo." voices=""/>
    <char @l="310" id="mit_" pos="auto" src="mit_aaej"/>
    <msg @l="312" char_id="-1" frame_id="-1" name="" text="“Parece cansado”." voices="mitu00828"/>
