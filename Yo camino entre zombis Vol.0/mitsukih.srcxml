    <msg @l="321" char_id="-1" frame_id="-1" name="" text="Al parecer, había ocurrido un accidente de tráfico más&#xA;adelante, y por eso el autobús tuvo que esperar un rato." voices=""/>
    <msg @l="322" add="true" char_id="-1" frame_id="-1" name="" text=" Los&#xA;pasajeros, irritados, golpeaban sus teléfonos a su alrededor." voices=""/>
    <msg @l="323" char_id="-1" frame_id="-1" name="" text="Mitsuki estaba más preocupada por lo cansado que parecía&#xA;Yuusuke." voices=""/>
